  padding: 25px;
  margin: 0 auto 15px;
  @media (max-width: 479px) {
    padding: 5%;
  }
  .subcollection-header {
    h4 {
      font-size: $size-h5;
      margin: 0 0 16px;
    }
    h5 {
      color: $main-color;
      font-size: $body-font-size - 1px;
      font-weight: 600;
      font-family: $font-standard;      
    }
    span {
      display: block;
      text-align: end;
    }
  }
}
.subcollection-inner {
