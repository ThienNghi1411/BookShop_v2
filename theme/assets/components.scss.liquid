  margin: 0 -15px;
  padding: 0;  
  @extend %flexbox;
  @extend %flex-wrap;
  @media (max-width: 575px) {
    @include margin-start(-7.5px);
    @include margin-end(-7.5px);    
  }
  .mode-view-item {
    @extend %transition-ease;
    @extend %flexbox;
    padding: 0 15px;
    letter-spacing: 0;
    @media (max-width: 575px) {
    padding: 0 7.5px; 
    }
  }
}
.subcollection-wrapper {
