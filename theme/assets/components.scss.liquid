  @include margin-end(-7.5px);
  @extend %flexbox;
  @extend %flex-wrap;
  > .subcollection-item {
    padding: 0 7.5px;
    @media (min-width: 768px) {
      @include flex(0 0 25%);
      max-width: 25%;      
    }
    @media (max-width: 767px) {
      @include flex(0 0 25%);
      max-width: 25%;      
    }
  }
  .subcollection-item {
    a {
      display: block;

    }
    .subcollection-title {
      font-size: $body-font-size - 1px;      
      color: $body-light-color;
      margin: 13px auto 0; 
      @media (min-width: 1025px) {
        &:hover {
          color: $main-color;
        }
      }
    }
  }
  &.owl-loaded {
    @include margin-start(0);
    @include margin-end(0);
  }
}
.templateCollection {
  .top-collection {
    position: relative;   
    background: $body-second-color;    
    margin: 0 auto 15px;
  }
  .cata-description {
    padding: 25px 0;
    font-size: $body-font-size;
    color: $body-light-color;
    line-height: 24px;
    @media (max-width: 479px) {
      display: none;
    }
  }
  .top-banner {
    img {
      width: 100%;
    }    
  }
  .page-cata {
    padding-bottom: 50px;
    .cata-toolbar {
      @media (max-width: 991px) {
        display: none;
      }
    }
    .col-12 {
      @include scroll-bar-ul;
    }
    .mobile-layout-bar {
      .cata-toolbar {
        display: block;
        .sort-by {
          @include padding-start($padding-inset-sm);
          @media (max-width: 479px) {
            max-width: 50%;
          }
        }
      }
    } 
    &.order-form {
      .cata-toolbar {
        @media (max-width: 991px) {
          .filter-icon {
            color: $main-color;
          }
        }
      }
    }
    .products-grid-item {      
      .product-wrapper:not(.product-style-1) {
        .product-head {
          .product-button {
            display: none;
          }
        }
      }
    }
  }  
}
