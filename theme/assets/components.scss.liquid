    @extend %flexbox;
    @extend %align-items-center;
    .modal-content {
      background: $body-second-color;
      padding: 5%;
      border: none;      
      @extend %border-radius-reset;
      &.s-loading {
        .navbar-form {
          &:before {
            z-index: 5;
            font-size: $font-small-icon;
            vertical-align: middle;
            font-family: "arenafont";
            content: "\e89c";            
            color: $body-title-color;
            width: 40px;
            height: 50px;
            text-align: center;
            line-height: 50px;
            @include ab(0px, n, n, 0);
            @extend %animation-fa-spin;           
          }
          .search-icon {
            @extend %opacity-zero;
          }
        }
      }
    }
    button.close {      
      @extend %btn-close;
    }
    h3 {
      margin: 0;
      padding: 0 0 15px;
      font-size: $size-h4;
    }
    .navbar-form {
      padding: 0;
      margin: 0;
      position: relative;
      .form-control {
        width: 100%;                     
        font-weight: 300;
        @include padding-end(45px);
      }
      .search-icon {
        border: none;
        padding: 0;
        margin: 0;
        background: none;
        width: 50px;
        height: 50px;
        line-height: 50px;
        text-align: center;       
        z-index: 8; 
        @include ab(0, n, n, 0);        
        i {
          font-size: $font-icon;
          color: $body-general-color;
        }
        &:hover {
          i {
            color: $main-color;
          }
        }
      }
