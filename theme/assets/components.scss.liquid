      margin: 0 0 30px;
      .product-head {
        display: table-cell;
        width: 110px;
        vertical-align: middle;
        @include padding-end($padding-inset-sm);
        img {
          width: 100%;
        }
      }
      .product-content {
        display: table-cell;
        vertical-align: middle;
        padding: 0 !important;
        text-align: start;
        .product-name {
          text-transform: none;
          height: auto !important;
          font-size: $body-font-size;
          margin-bottom: 5px;
        }
        .pc-inner {
          position: relative;
          height: 100%;
          padding: 0 95px 0 0;
          @media (max-width: 1199px) {
            padding: 0;
          }
        }
        .product-author {
          font-size: $body-font-size - 2px;
          line-height: 1.4;          
          a {
            color: $body-light-color;
            &:hover {
              color: $main-color;
            }
          }
        }
        .product-variants {
          padding: 5px 0 0;
          font-size: $body-font-size - 2px;
          color: $body-light-color;
        }
        .product-price {
          margin: 5px 0 0;
        }
        .product-price,
        .price,        
        .price-sale {
          font-size: $body-font-size;
        }
        .price-compare {          
          font-size: $body-font-size - 2px;
        }
      }
    }
  }
}
 .owl-controls {
  .owl-nav {    
    > div {
      z-index: 99;
      background-image: none;
      text-align: center;
      border: none;
      font-size: $font-icon;
      width: 55px;
      height: 55px;
      line-height: 55px;
      background: $light-color;
      color: color-contrast($light-color);	            
      cursor: pointer;					
      @extend %border-radius-circle;	
      @extend %transition-ease;	 
      @include box-shadow(0 0 18px rgba($dark-color, 0.16));
      @include ab(30%, n, n, n);
      @media (max-width: 767px) {
        width: 30px;
        height: 30px;
        line-height: 30px;
      }
      span {
        display: none;
      }
      &:before {
        font-family: "arenafont";
        color: inherit;
      }   
      &.owl-next {     
        right: 0;
        @media (min-width: 1240px) {
          right: -13px;
        }
        &:before {
          content: "\e82c";	          
        }
      }
      &.owl-prev {   
        left: 0;
        @media (min-width: 1240px) {
          left: -13px;
        }     
        &:before {
          content: "\e82f";	
        }
      }
      &:hover {
        background: $main-color;
        color: color-contrast($main-color);
      }
    }
    @media (max-width: 1024px) {
      display: none;
    }
  }
  .owl-dots {
    text-align: center;
    margin-top: 10px;
    .owl-dot {
      display: inline-block;
      margin: 0 5px;
      span {
        background: rgba($main-color, 0.5);
        width: 12px;
        height: 12px;
        display: block;  
        @extend %border-radius-circle;    
        @extend %transition-ease;
        &:hover {
          @media (min-width: 1025px) {
            background: $main-color;              
          }
        }  
      }
      &.active {
        span {
          background: $main-color;
        }
      }
    }
  }
}
.pagination-holder { 
  .pagination {
    margin: 0 auto; 
    flex-wrap: wrap;
    @extend %flexbox;
    @extend %border-radius-reset;    
    @extend %justify-content-center;    
    > li {
      margin: 0 7px 7px;            
      > a,
      > span {
        display: block;
        padding: 8px;              
        line-height: 35px;
        min-width: 55px;
        height: 55px;
        text-align: center;        
        font-size: $size-h5;      
        color: $body-title-color;
        border: 2px solid transparent;
        @extend %border-radius-circle;    
      }      
      &:hover {
        > a,
        > span {
          @media (min-width: 1025px) {
            border-color: $main-color;
            color: $main-color;
            background: $body-second-color;
            @include box-shadow(0 0 18px rgba($main-color, 0.36));
          }
        }
      } 
      &.active {
        > a {
          border-color: $main-color;
          color: $main-color;
          background: $body-second-color;
          @include box-shadow(0 0 18px rgba($main-color, 0.36));
        }
      } 
      &.paginate-next, &.paginate-prev {
        >a, >span {
          border: 2px solid transparent;
        }
        &:hover {
          >a, >span {
            border: 2px solid transparent;
            @extend %box-shadow-reset;
            @extend %border-radius-reset;
          }
        }
      }               
    }
  }
}
.fancybox-slide {
  padding: 20px;  
  .fancybox-content {
    overflow: visible;
  }   
}
.modal-dialog {
  max-width: 780px;
  .modal-body {
    padding: 0;
  } 
}
#newsletter-popup { 
  width: 780px;
  z-index: 9999;
  margin: 0 auto;      
  @extend %border-radius-5;
  @media (max-width: 910px) {      
    width: 100%;      
  }
  .text-box-image {
    height: 100%;
    max-height: calc(100vh - 40px); 
    width: 100%;
    overflow-y: auto;
    @extend %flexbox;
    @extend %flex-wrap;
    .block-image {        
      /*position: relative;*/ 
      span {
        height: 100%;          
      }
      img {
        @include ab(0, 0, n, n);
        height: 100%;
        width: 100%;
        object-fit: none;
        font-family: "object-fit: cover";
      }
    }
  }
  .newsletter-popup-content {
    display: flex;
    min-height: 420px;
    align-items: center;      
    width: 100%;
    height: 100%;
    max-height: 95vh;   
    position: relative;  
    padding: 15px;      
    @extend %border-radius-5;
    .np-close {
      @include ab(-10px,n,n,-10px);
      width: 30px;
      height: 30px;
      line-height: 30px;
      text-align: center;
      cursor: pointer;
      background: $body-second-color;
      color: $dark-color;
      z-index: 1111;
      @extend %border-radius-circle;
      @include box-shadow(0 0 10px rgba($dark-color, 0.5));
    }
  }
  .block-image-true {
    .text-box {  
      &.position-right {
        @media (min-width: 768px) {
          @include flex(0 0 53%);
          @include flexbox;
          @include flex-direction(column);
          @include border-radius(5px, 5px, 5px, 5px);
          max-width: 53%;    
          @include margin-start(0);
          @include margin-end(0);  
          order: 2; 
        }
        @media (max-width: 767px) {
          float: none;
          width: calc(100% - 30px);
          @include margin-start($padding-inset-sm);
        }  
      }
      &.position-left {                    
        @media (min-width: 768px) {
          @include flex(0 0 53%);
          @include flexbox;
          @include flex-direction(column);
          @include border-radius(5px, 5px, 5px, 5px);
          max-width: 53%;         
        }
      }
    }
    .block-image {
      &.image-position-right {
        @media (min-width: 768px) {
          @include flex(0 0 47%);
          max-width: 47%;
          right: 47%;
          order: 1;
        }
        @media (max-width: 767px) {
          display: none;
        }
      } 
      &.image-position-left {
        @media (min-width: 768px) {
          @include flex(0 0 47%);
          max-width: 47%;
        }
      }    
    }   
  }
  .box-countdown {
    overflow: hidden;
    @extend %border-radius-5;
  }
  .text-box {
    display: block;
    position: relative;
    padding: 20px 15px 15px;   
    z-index: 999;  
    background: $body-background-color;
    background-repeat: no-repeat;
    background-size: cover; 
    @media (max-width: 767px) {
      padding: 10px 5px 5px;
      width: 100%;
    }
    .text-box-content {
      margin-bottom: 20px;  
      @media (max-width: 767px) {
        margin-bottom: 15px;
      } 
      &:last-child {
        margin-bottom: 0; 
      }
    }
    .bold-text-true {
      font-weight: 700; 
    }
    input {
      width: 100%;
    }
    button {
      margin: 20px auto 0;       
    }
    .button-position-bottom {
      input {
        width: 100%;
        display: block;
        margin-bottom: 20px;
      }
      button {
        width: 100%;
      }
    }
    .button-position-right {
      form {
        display: block;
        width: 100%;       
        position: relative;        
        text-align: center;           
      }
    }
    .block-social {
      text-align: center;
    } 
    .widget-social {
      margin: 0; 
      &.border-top-true {
        border-top: 1px solid $body-border-color;
        padding-top: 20px;          
      }
      li:not(:last-child) {
        @include padding-end($padding-inset-xxs);
      }
      a {
        background: $body-border-color;
        color: $body-link-color;
        span {
          font-size: 0;
        }
      }
      i {
        font-size: $font-icon;
      }
    }
    .countdown-content {        
      text-align: center;
      padding: 15px;        
      .offer-text {
        margin-bottom: 10px; 
        color: inherit;
        font-size: 120%;     
      }
      .countdown-row {
        @extend %flexbox;
        @extend %align-items-center;    
        @extend %justify-content-center;
      }
      .countdown-section {
        padding: 0 15px;          
        font-size: 120%;          
        width: auto;
        @media (max-width: 479px) {
          @include padding-start(7px);
          @include padding-end(7px);
        }
        .countdown-amount {
          background: none;
          color: inherit; 
          font-size: inherit;
        }
        .countdown-period {
          color: inherit; 
          font-size: $body-font-size - 1px;
        }
      }
      .popup-countdown {
        @include flexbox;
        justify-content: center;
        >span {
          padding: 0 15px;
        }
        .date-number {
          display: block;
        }
      }
    } 
  }
  .subscribe-result {
    .result-inner {
      padding: 45px 40px 40px;
      text-align: center;
      button {
        padding: 8px 50px;
        border: 1px solid currentColor;
      }
    }  
    .text-1 {
      margin: 0 0 30px;
      font-size: $size-h2;
      font-weight: 700;
    } 
    .coupon-code {
      margin: 0 0 30px;
      .text-2 {
        margin-bottom: 15px; 
      }  
      .code {
        font-weight: 700; 
      } 
    } 
  }
}   


/*sell-people*/
.popup-sell-people {
  @include border-radius(5px, 5px, 5px, 5px);
  @include box-shadow(0 0 5px 0 #c6c6c6);
  width: 350px;
  display: table;
  vertical-align: middle;
  position: fixed;
  z-index: 9999;
  display: none;
  @media (max-width: 399px) {
    width: 100%;    
  }
  &.show {
    display: block;
  }
  &.bottom-left {
    bottom:10px;
    left:10px;
    @media (max-width: 991px) {
      bottom: 64px;
    }
    @media (max-width: 414px) {
      left: 0;
    }
  }
  &.bottom-right {
    bottom: 10px;
    right: 10px;
    @media (max-width: 991px) {
      bottom: 64px;
    }
  }
  &.top-left {
    top:10px;
    left:10px;
  }
  &.top-right {
    top: 10px;
    right: 10px;
  }
  .img-user {
    width: 80px;
    display: table-cell;
    vertical-align: middle;
    img {
      width: 100%;
      @include border-radius(5px, 0, 0, 5px);
    }
  }
  .text-detail {
    display: table-cell;
    vertical-align: middle;
    padding: 10px 15px;
    .sell-text {
      margin: 0;
      line-height: 20px;
      font-size: $body-font-size;
      font-weight: normal;
      color: $body-general-color;
    }
  }
}
