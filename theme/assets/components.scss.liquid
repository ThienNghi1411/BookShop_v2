}
/* Sidebar cart ------------------------*/
.cart-sb, .cart-dd {
	margin: 0;
	padding: 34px;
	width: 370px;
	position: absolute;
	background: $body-second-color;
	@include transition(all 0.35s ease);
	@include ab(100%, n, n, 0);
	left: auto;
	text-align: start;
	color: $body-title-color;
	@media (max-width: 370px) {
		width: 100%;
		padding: 0 5%;
	}
	#cart-info {
		.cart-item-empty {
			padding: 10px 20px;
			p {
				margin: 0;
			}
		}
		h1{
			text-transform: none;
			margin: 0;
			padding: 15px 15px 0;
		}
		.items-inner {			
			position: relative;			
			width: 100%;
			margin: 0 0 20px;			
			padding-bottom: 20px;
			@extend %flexbox;
			@extend %align-items-center;
			&:not(:last-child) {
				&:after {
					content: '';
					width: 60%;
					height: 1px;
					background: $body-border-color;
					@include ab(n, 0, 0, n);
				}
			}			
			.cart-item-image {
				float: none;				
				vertical-align: middle;
				max-width: 110px;
				padding: 0 20px 0 0;
				text-align: center;
				@include flex(0 0 110px);
			}
			.cart-item-info {
				float: none;								
				@include flex(1);
			}
			.cart-item-quantity {
				padding: 0 0 8px;
				span {
					color: $body-general-color;
				}
			}
			.cart-close {										
				text-align: end;
				max-width: 30px;
				@include flex(0 0 30px);
				i {
					color: $body-general-color;
					font-size: $size-h4;
				}
				&:hover {
					i {
						color: $main-color;
					}
				}
			}
			.cart-item-title {
				margin: 0;
				padding: 0 0 15px 0;
				line-height: 1.2;
				a {
					font-size: $body-font-size + 2px;
					color: $body-general-color;									
					&:hover {
						color: $main-color;
					}
				}
			}
			.cart-item-price {
				color: $body-price-color;
				line-height: 1.3;
				font-size: $body-font-size;		
				font-weight: 300;		
			}
		}
		.total {			
			margin-top: 15px;
			width: 100%;
			@extend %flexbox;
			@extend %align-items-center;
			@extend %justify-content-between;			
			> span {
				font-size: $size-h3;
				color: $body-title-color;
			}
		}
	}
	.action {		
		margin: 0;
		padding-bottom: 5px;
		.btn {
			width: 100%;
			margin: 20px 0 0;
		}
	}
}
.cart-dd {
	visibility: hidden;
	z-index: 0;
	display: block;
	right: -28px;
	@extend %opacity-zero;
	@include translate(0, 20px);
	@include box-shadow(0 0 18px rgba($dark-color, 0.21));	
	&:before {
		content: "";
		width: 100%;
		height: 15px;
		@include ab(-12px, 0, n, n);
	}
	&:after {
		content: "";
		border-bottom: 10px solid $body-border-color;
		@include border-inline-start(9px solid transparent); 
		@include border-inline-end(9px solid transparent);
		@include ab(-10px, n, n, 31px);
		z-index: 100;
	}
	.items {
		max-height: 340px;
		overflow-y: auto;
	}	
}
.cart-sb {
	padding-top: 0;
	position: fixed;
	top: 0 !important;
	right: 0 !important;
	z-index: 9999999;
	height: 100%;
	display: block;
	overflow-y: auto;
	opacity: 0;
	@include translate(375px, 0);	
	@include transition(all 0.7s cubic-bezier(0.25, 0.46, 0.45, 0.94));	
	.cart-sb-title {
		padding: 0;
		position: relative;
		background: $button-btn1;
		color: color-contrast($button-btn1);
		margin: 0 -34px;
		@media (max-width: 370px) {
			margin-left: -5%;
			margin-right: -5%;
		}
		.c-close {
			font-size: $font-small-icon;
			display: inline-block;
			cursor: pointer;
			text-align: center;
			color: inherit;
			width: 45px;
			height: 45px;
			line-height: 45px;
			@include ab(0px, 10px, n, n);			
			@extend %transition-ease;					
		}
		.c-title {
			color: inherit;
			font-size: $size-h4;
			font-weight: 500;			
			line-height: 45px;
			text-align: center;
			display: block;
		}
	}
	.cart-order-note {
		display: none;
	}
	.wrap-action {
		@include ab(n, 34px, 34px, 34px);
		background: $body-second-color;
		@media (max-width: 370px) {
			left: 5%;
			right: 5%;
			bottom: 20px;
		}
	}
	form {
		display: block;
		width: 100%;
		height: 100%;
	}
	.cart-content {
		opacity: 0;
		visibility: hidden;
		@include transition(all 0.2s ease-out 0s);		
	}
	.cart-order-note .cart-note textarea {
		min-height: 90px;
	}
	#cart-info {
		max-height: calc(100% - 300px);
		border: 0;
		overflow-y: auto;
		.items {
			overflow-y: auto;
			padding-top: 20px;
		}
		.items-inner {
			@include translate(0, 100px);
			@extend %opacity-zero;			
		}
		@for $index from 0 through 18 {
			.animated-#{$index}{
				@include transition(transform 1s cubic-bezier(0.165,.84,.44,1) calc(0.1s + calc(0.12s  #{$index})), opacity 1s cubic-bezier(.165,.84,.44,1) calc(0.2s + calc(0.12s  #{$index})) )
			}
		}
		.cart-item-empty {
			padding-top: 10px;
			text-align: start;
		}
	}
	.cart-order-note {
		padding-top: 15px;
		@media (max-width: 767px) {
			display: none;
		}
		span {
			display: block;
			padding-bottom: 5px;
			font-size: $body-font-size - 1px;
		}
	}
	&.opened {
		@include translate(0, 0);
		margin-top: 0 !important;
		opacity: 1;
		.c-close {
			@extend %opacity;
		}
		#cart-info .items-inner {
			@extend %opacity;
			@include translate(0, 0);
		}
		.cart-content{
			opacity: 1;
			visibility: visible;
		}
		@media (max-width: 319px) {
			max-width: 250px;
		}
	}
}
/*Free shipping*/
.cart-sb .wrap-total {
	margin-top: 20px;
	.sb-progress {
		@extend %load_progressbar;
	}
	.sb-progress-bar,
	.sc-progress-bar {
		@extend %per_progressbar;
		color: color-contrast($main-color);
	}
	.sb-text {
		margin-top: 20px;
		span {
			display: inline-block;
		}
	}
}

.group-toolbar {
  @extend %flexbox;
  @extend %align-items-center;
  @extend %justify-content-between;   
  .filter-icon {        
    cursor: pointer;
    font-size: $size-h4;
    color: $body-light-color;
    @extend %flexbox;
    @extend %align-items-center;
    @extend %transition-ease;
    @media (max-width: 991px) {
      color: color-contrast($header-mobile-bg);
    }
    @media (max-width: 479px) {
      font-size: 14px;
    }
    i {
      @include margin-end($padding-inset-xxs);
      font-weight: 400;
      font-size: 16px;
      @media (min-width: 992px) {
        color: rgba($body-general-color, 0.5);
      }       
      &:before {
        position: static;
      }
    }
    &:hover {
      @media (min-width: 1025px) {
        color: $body-title-color;
      }
    }
  }
}
.cata-toolbar {
  margin: 0 0 20px;
  width: 100%;  
  position: relative;  
  @media (max-width: 991px) {
    margin: 0;
    padding: 10px;
  }
  .group-gl {
    float: right;
    margin: 0 0 10px 0;
  }
  .grid-list {        
    @extend %flexbox;
    @extend %align-items-center;
    .grid, .list {
      text-align: center;      
      cursor: pointer;      
      color: rgba($body-general-color, 0.5);
      @include padding-end($padding-inset-sm); 
      @media (max-width: 991px) {
        height: 26px;        
        color: rgba(color-contrast($header-mobile-bg), 0.5);
      }
      @media (max-width: 374px) {
        @include padding-end($padding-inset-xs);
      }
      i {
        font-size: $font-small-icon;
        display: block;
        &:before {
          width: auto;
        }
      }
      &.active {
        background: none;
        opacity: 1;
        @media (min-width: 992px) {
          color: $main-color; 
        }       
      }
      &:hover {
        background: none;
        @media (min-width: 1025px)  {
          color: $main-color;   
        }
      }  
    } 
  }
  .group-toolbar {    
    >div~.grid-list {
      .grid, .list {
        @include padding-start($padding-inset-sm);
        @include padding-end(0);
        @media (max-width: 374px) {
          @include padding-start($padding-inset-xs);
          @include padding-end(0);
        }
      }
    }
  }
  .filter {
    @include margin-end(40px);
    padding-bottom: 20px;
    margin-bottom: -1px;
    &:hover {
      > .dropdown-menu {
        display: block;
        z-index: 11;
      }
    }
    > a {
      border: 1px solid $body-border-color;
      padding: 9px 20px;
      font-size: 14px;
      display: inline-block;
      width: 140px;
      text-align: center;
      i {
        float: right;
        margin-top: 4px;
      }
      &:hover{
        text-decoration: none;
        color: $body-link-color;
      }
    }
    > .dropdown-menu {
      right: auto;
      left: 15px;
      width: 100%;
      padding: 25px 30px;
    }
    .clear-filter{
      display: none;
    }
  }
  .sort-by {    
    color: $body-general-color;
    line-height: 28px;  
    > label{
      @include margin-end($padding-xxs);
      vertical-align: top;
      margin-bottom: 0;
      display: inline-block;
    }
    .sort-by-inner {      
      position: relative;      
      white-space: nowrap;
      @extend %align-items-center;
      @extend %flexbox;
      @media (max-width: 991px) {       
        select {
          height: 40px;
          line-height: 38px;
          max-width: 100%;
          color: color-contrast($header-mobile-bg);
        }
        &:before {
          content: '\e817';
          @include font-family;
          @include ab(n,n,n,15px);
          color: color-contrast($header-mobile-bg);
        }
      }
      @media (max-width: 400px) { 
        .sort-by-title {
          display: none;
        }
      }
      label {        
        margin: 0 10px 0 0;
        color: $body-general-color;
        line-height: 28px;
        font-size: $size-h4;
        letter-spacing: 0.72px;
      }
      #cata_sort_by {
        float: right;
        position: static;
        &:after {
          content: none;
        }
        button {
          float: left;
          border: none;
          padding: 0 5px 0 0;
          background: none;
          text-align: start;
          line-height: 28px;
          white-space: nowrap;
          font-size: $size-h4;
          cursor: pointer;
          @media (max-width: 350px) {
            font-size: $body-font-size;
          }
          a {
            color: rgba($body-general-color, 0.5);
            letter-spacing: 0.72px;       
          }
          span {
            padding: 0 10px;
            float: left;            
            @media (max-width: 350px) {
              padding: 0;
            }
            &.visible-xs {
              padding-top: 0;
            }
          }
          i{
            float:right;
            text-align: end;
            padding: 0;
            content: none;
            color: $body-light-color;
            font-size: 14px;
          }
        }
      }      
    }
    &.has-border {
      #cata_sort_by {
        border: 1px solid $body-light-color;
        padding: 5px;
        @extend %border-radius-5;
      }
    }
    #sort_by_box {
      width: auto;
      min-width: 0;
      background: $light-color;
      padding: 10px 18px;
      display: block;
      visibility: hidden;
      z-index: -99;
      list-style: none;
      @include ab(100%, -1px, n, -1px);
      @include translate(0%, 10px);
      @extend %opacity-zero;
      @include  transition(all 0.35s ease);
      @include box-shadow(0 0 18px rgba($dark-color, 0.21));
      @media (max-width: 991px) {
        @include translate(0%, -100%);
      }
      li{
        padding: 0;
        text-align: start;
        margin-bottom: 0;        
        a {
          padding: 4px 0;
          line-height: 20px;
          display: block;
          transition: none;
          font-size: $body-font-size - 1px;
          color: $body-general-color;
        }
        &:hover {
          @media (min-width: 1025px) {
            a{
              color: $main-color;
            }
          }
        }
        &.active {
          a{
            cursor: default;
            color: $main-color;
          }
        }        
      }
      @media (max-width: 479px) {
        padding: 10px;
        li {
          a {
            font-size: 12px;
          }
        }
      }
    }
    &.opened {
      #sort_by_box {
        visibility: visible;
        z-index: 99;
        @extend %opacity;
        @include box-shadow(0 0 18px rgba($dark-color, 0.16));
        @media (max-width: 991px) {
          @include translate(0%, -110%);
        }
      }
    }
  }
  .top-pagination-holder {
    float: right;
    margin: 5px 0;
    > span {
      float: left;
      line-height: 40px;
      padding: 0 7px 0 0;
    }
    .pagination {
      float: left;
    }
  }
  .visible-xs{
    cursor: pointer;
  }
}
