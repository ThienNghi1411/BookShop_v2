  z-index: 9999;        
  color: $body-general-color;     
  background: $body-background-color;
  @extend %border-radius-circle;
  @extend %opacity;
  @extend %transition-ease;
  @include ab(-15px, n, n, -15px);      
  @include box-shadow(0 0 10px rgba($dark-color, 0.5));
  svg, i {
    display: none;
  }
  &:before {
    content: '\e831';
    font-family: 'arenafont';    
    font-size: 14px;        
    color: inherit;
    display: block;
  } 
  &:hover {
    background: $main-color;
    color: $light-color;
  }
}
