%countdown-product {
  .wrapper-countdown {
    margin: $padding-xs 0;
    > div{
      margin: 0;
      padding: 0;
      background: none;
      border: none;
    }
    .countdown-row {
      display: inline-block;
      text-align: start;
      width: auto;
      padding: 0;
      .countdown-section {
        width: 65px;
        color: color-contrast($main-color);
        &:not(:last-child) {
          @include margin-end($padding-inset-sm);
          @media (max-width: 767px) {
            @include margin-end($padding-inset-xs);
          }
        }
        @media (max-width: 767px) {
          width: 55px;
        }
        .countdown-period {
          background: rgba($main-color, 0.8);
          padding: 8px 5px;
          @include border-radius(0, 0, 5px, 5px);
        }
      }
      .countdown-amount {
        position: relative;   
        font-weight: 700;  
        display: block;
        font-size: $size-h3;
        background: $main-color;
        padding: 8px 5px;
        @include border-radius(5px, 5px, 0, 0);
        @media (max-width: 767px) {
          font-size: $size-h3 - 4px;
        }
      }
    }
  }
}
%quantity-product {
  text-align: center;
  position: relative; 
  padding: 3px 54px;          
  input[type="number"] {
    -moz-appearance: textfield;
    -webkit-appearance: textfield;            
  }
  input[type=number]::-webkit-inner-spin-button,
  input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;    
  } 
  input {
    width: 60px;
    height: 44px;
    padding: 9px 5px;
    text-align: center;    
    background-repeat: no-repeat;
    background-attachment: fixed;
    background: transparent;    
    border: 2px solid #5e5e5e;                                                
  }
  .qty-inner > span {
    height: 42px;
    line-height: 38px;
    width: 42px;
    cursor: pointer;
    font-size: $body-font-size + 2px;     
    border: 2px solid $body-border-color;
    @extend %border-radius-circle;            
    @extend %transition-ease;
    @include ab(3px, n, n, n);            
    &.qty-down {                                          
      left: 0;
    }
    &.qty-up {
      right: 0;
    }
    i {
      color: inherit;
    }            
    &:hover {
      border-color: $main-color;
      color: $main-color;
    }            
  }
}
%anmation-duration {
  -webkit-animation-duration: .5s;
  animation-duration: .5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@mixin pc-header-fixed {
  position: fixed;
  z-index: 9999;
  top: 0;
  left: 0;
  right: 0;
  -webkit-animation-name: fadeInDown;
  animation-name: fadeInDown;  
}
@mixin effect-fade-in_out {
  content: "";
  @include ab(n,0,0,0);
  width: 100%;
  height: 100%;
  @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
  background: rgba($overlay-color, 0.25);
  z-index: 1;
}
@mixin scroll-bar-ul { 
  scrollbar-color: $main-color $body-second-color;
  scrollbar-width: thin; 
  .product-playlist::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar-thumb {
    background-color: $main-color;
  }
}
@mixin scroll-bar-search { 
  scrollbar-color: $main-color $body-second-color;
  scrollbar-width: thin; 
  ul::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar-thumb {
    background-color: $main-color;
  }
}
#searchModal {
  @extend %flexbox;
  @extend %align-items-center;
  background: rgba($dark-color, 0.76) none repeat scroll 0 0;
  position: fixed;    
  height: 100%;  
  top: 0;
  left: 0;
  display: none;
  padding: 5% 0;
  z-index: 9999;  
  #searchModal[aria-hidden="false"] {
    z-index: 99999;
  } 
  .modal-dialog {
    width: 90%;
    max-width: 580px;
    min-width: 290px;
    height: 100%; 
    padding: 0;
    margin: 0 auto;    
    position: relative;
