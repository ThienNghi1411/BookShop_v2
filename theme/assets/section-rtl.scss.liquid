}
%text-line-clamp {
  overflow: hidden;
  display: -webkit-box;
  display: -ms-box;
  -webkit-box-orient: vertical;  
}
%btn-close {
  width: 35px;
  height: 35px;
  line-height: 35px;
  margin: 0;
  padding: 0;
  text-align: center;  
  cursor: pointer;
  z-index: 9999;        
  color: $body-general-color;     
  background: $body-background-color;
  @extend %border-radius-circle;
  @extend %opacity;
  @extend %transition-ease;
  @include ab(-15px, n, n, -15px);      
  @include box-shadow(0 0 10px rgba($dark-color, 0.5));
