}
%anmation-duration {
  -webkit-animation-duration: .5s;
  animation-duration: .5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@mixin pc-header-fixed {
  position: fixed;
  z-index: 9999;
  top: 0;
  left: 0;
  right: 0;
  -webkit-animation-name: fadeInDown;
  animation-name: fadeInDown;  
}
@mixin effect-fade-in_out {
  content: "";
  @include ab(n,0,0,0);
  width: 100%;
  height: 100%;
  @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
  background: rgba($overlay-color, 0.25);
  z-index: 1;
}
@mixin scroll-bar-ul { 
  scrollbar-color: var(--main-color) $body-second-color;
  scrollbar-width: thin; 
  .product-playlist::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar-thumb {
    background-color: var(--main-color);
  }
}
@mixin scroll-bar-search { 
  scrollbar-color: var(--main-color) $body-second-color;
  scrollbar-width: thin; 
  ul::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar-thumb {
    background-color: var(--main-color);
  }
}
.banner-item {     
