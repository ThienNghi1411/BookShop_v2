	&.policy-display-style-2 {	
		position: relative;
		@include box-shadow(0 6px 15px 0 rgba($dark-color, 0.15));							
		.policies-content {
			.policy-item {								
				padding: 40px 25px;
				@extend %flexbox;
				@extend %align-items-center;
				@media (max-width: 575px) {
					padding: 6% 5%;
				}
				.media {
					@extend %align-items-center;
				}
				.policy-icon,
				.policy-image {	
					@include margin-end(14px);
					min-width: 40px;								
				}
				.policy-image {
					img {
						max-width: 40px;
					}
				}
				.policy-icon {
					i {
						color: var(--main-color);
						font-size: 40px;
					}
				}
				.text {	
					@include padding-start(6px);
					h4 {
						padding: 0 0 10px;
						letter-spacing: 0.36px;	
						font-weight: 500;					
					}							
					span {
						display: block;
						font-size: $body-font-size - 2px;
						font-weight: 300;
					}
				}			
			}
			&.row-here {
				&.policies-content {
					@include padding-start($padding-inset-sm);
					@include padding-end($padding-inset-sm);			
				}
			}
		}
		&.has-border {
			.policy-item {
				@include border-inline-start(1px solid $body-border-color);
				&:last-child {
					@include border-inline-end(1px solid $body-border-color);
				}
			}
			.type-carousel {
				.policy-item {
					@include border-inline-start(0);
				}
				.owl-stage {
					>div:first-child {
						.policy-item {
							@include border-inline-start(1px solid $body-border-color);
						}
					}
				}
			}
		}		
	}
	&.policy-display-style-3 {
		.policies-content {
			.policy-item {
				margin-bottom: 20px;
			}
			.policy-item-inner {
				text-align: center;
				padding: 15px;
				@media (max-width: 767px) {
					@include padding-start(0);
					@include padding-end(0);
				}
			}
		}
	}
	&.policy-display-style-3, &.policy-display-style-4 {
		.policies-content {
			.policy-item-inner {
				.policy-icon {
					width: 48px;
					height: 48px;
					line-height: 48px;
					text-align: center;
					background: var(--main-color);
					color: color-contrast(var(--main-color));	
					margin: 0 auto;
					@extend %border-radius-circle;	
					i {
						color: inherit;
						font-size: 30px;
					}
				}
				.policy-image  {
					width: 48px;
					margin: 0 auto;
					img {
						@extend %border-radius-circle;
					}
				}
				.text {					
					padding: 16px 0 0;
					h4 {
						line-height: 150%;
						margin-bottom: 8px;
					}
					span {
						display: block;
						font-size: $body-font-size - 2px;
						line-height: 150%;
						font-weight: 300;
					}
				}
			}
		}
		&.has-border {
			.policy-item {
				.policy-item-inner {
					border: 1px solid $body-border-color;
				}
			}
		}
	}
	&.policy-display-style-4 {
		.policies-content {
			padding-top: 10px!important;
			padding-bottom: 10px!important;
		}
		.policy-item {
			.policy-item-inner {
				text-align: center;
				padding: 30px;
				margin: 5px 0;
				@include box-shadow(0px 0px 5px 1px rgba($dark-color, 0.07));
				@media (max-width: 991px) {
					margin-bottom: 15px;
				}
				.text {
					h4 {
						margin-top: 8px;
						margin-bottom: 0;
					}
				}
			}
		}
	}
	.policies-content {	
		overflow: visible!important;	
		&.row-here {			
			.policy-item {				
				@media (max-width: 479px) {
					@include flex(0 0 100%);
					max-width: 100%;					
				}
			}
			@media (max-width: 575px) {
				.row {
					margin: 0 -7.5px;
					>div {
						padding-left: 7.5px;
						padding-right: 7.5px; 
					}
				}
			}
		}
		.policy-item {
			width: 100%;
		}
		.carousel {
			height: 0;			
			@extend %opacity-zero;
			&.owl-loaded {
				height: auto;
				@extend %opacity;
			}
			.owl-stage,
			.owl-item {
				@extend %flexbox;
			}			
		}				
	}
	.container {
		.policies-content.row-here {
			padding: 0;
		}
	}
}
.special-products-list,
.swiper-container-horizontal {  
  @extend %flexbox;
  overflow: hidden;
}
.special-products-list {
  position: relative;   
}
.special-product-item {
  background: $background-pre-load;
  @extend %flexbox;
  @extend %align-items-center; 
  @media (min-width: 768px) {
    width: 700px;
  }
  @media (min-width: 992px) {
    width: 930px;
  }
  @media (min-width: 1200px) {
    width: 1170px;
  }
  @media (min-width: 1600px) {
    width: 1410px;
  }
  @media (max-width: 767px) {
    width: 100vw;
  }
  .bg-img {
    position: relative;
    height: 100%;
    width: 100%;
    span {
      height: 100%;
      min-height: 180px;
      img {
        height: 100%;
        @extend %border-radius-reset;
      }
    }
  }
  .media {  
    width: 80%;
    padding: 15px;      
    @extend %align-items-center;  
    @extend %flex-wrap;
    @media (max-width: 991px) {
      width: 100%;
    }
    @media (max-width: 479px) {
      padding: 15px 25px;
    }
    .product-image {        
      text-align: center;
      img {
        @include box-shadow(0 0 24px rgba($dark-color, 0.34));
      }
    }
    .pc-inner {
      h3 {
        margin: 0 auto 5px;
        line-height: 1.2;
        font-size: $size-h1;
        letter-spacing: 0.8px;
        font-weight: 600;
        @media (max-width: 1025px) {
          font-size: 3vw;
        }
        @media (max-width: 479px) {
          font-size: 5vw;
        }
        a {          
          text-transform: uppercase;          
        }			
      }	
      .product-price {
        font-size: $size-h3 + 1px;
        padding: 0 15px 15px 0;
        display: inline-block;
        @media (max-width: 479px) {
          padding: 0 10px 10px 0;
          font-size: 14px;
        }
        .price-sale {
          @include padding-start(10px);
        }
      }	
      .product-review {
        display: inline-block;
        position: relative;
        top: -2px;
        @media (max-width: 767px) and (min-width: 480px) {
          display: none;
        }
      }
      .product-vendor {
        line-height: 1.5;
        margin-bottom: $padding-xxs;
        .product-vendor__name {
          font-size: $size-h4;
          text-transform: uppercase;
          @include margin-start($padding-inset-xs);
        }
      }
      .product-button {
        @extend %flexbox;
        .btn-play {
          padding: 8px 30px;
          @media (max-width: 479px) {
            padding: 6px 20px;
            height: 42px;
          }
          i {
            @include margin-start($padding-inset-sm);
            font-size: $font-icon;
          }
        }
        .product-add-cart {
          width: 48px;
          height: 48px;
          line-height: 48px;
          text-align: center;
          @include margin-start($padding-inset-md);
          @media (max-width: 479px) {
            width: 42px;
            height: 42px;
            line-height: 42px;
          }
          a {
            display: block;
            height: 100%;
            font-size: $font-icon;
            @extend %border-radius-circle;
          }
        }
        .product-button__sharing {
          @include margin-start($padding-inset-md);
          >i {
            position: relative;
            display: block;
            width: 48px;
            height: 48px;
            line-height: 48px;
            text-align: center;
            font-size: $font-icon;
            @extend %border-radius-circle;
            @media (max-width: 479px) {
              width: 42px;
              height: 42px;
              line-height: 42px;
            }
          }
          .share-links {
            display: none;
            li {
              @include margin-end($padding-inset-xs);
              a {
                padding: 5px;
                width: 42px;
                height: 42px;
              }
            }
          }
          &.opened {
            .share-links {
              @include ab(n,n,n,n);
              padding-top: 20px;
            }
          }
        }
      }
      .spr-badge-starrating {
        @include margin-end($padding-inset-xs);
      }
      .product-author {
        font-weight: 300;
        font-size: $body-font-size + 4px;
        padding: 0 0 25px;
      }
      .btn {
        @include box-shadow(0 0 24px rgba($dark-color, 0.34));			
        border: none;
        font-size: $body-font-size + 2px;
        letter-spacing: 0.36px;
        @media (max-width: 1025px) {
          font-size: 14px;
          padding: 6px 20px;
          min-width: 150px;
        }
      }
    }
  }
}
.home-special-products {
  &.show-product-images-true {
    .bg-img {
      @media (max-width: 479px) {
        min-height: 640px;
        span {
          img {
            min-height: 640px;
          }
        }
      }
    }
    .product-image {
      @include flex(0 0 20%);
      max-width: 20%;
      @media (max-width: 991px) {
        @include flex(0 0 25%);
        max-width: 25%;
      }
      @media (max-width: 479px) {
        @include flex(0 0 80%);
        max-width: 300px;        
      }
    }
    .product-content {
      padding: 0 30px 0 50px;
      @include flex(0 0 80%);
      max-width: 80%;
      @media (max-width: 1199px) {
        padding: 0 30px;
      }
      @media (max-width: 991px) {
        @include flex(0 0 75%);
        max-width: 75%;
      }
      @media (max-width: 479px) {       
        max-width: 100%;
        @include flex(0 0 100%);
        padding: 20px 0 0;
      }
    }
  }
  .modal {
    .modal-dialog {
      @media (min-width: 576px) {
        max-width: 800px;
      }
      .close {
        @extend %btn-close;
      }
      .modal-body {
        padding: 15px!important;
        background: #e2e2e2;
        audio {
          width: 100%;
        }
      }
    }
  }
}
  .product-listing-section {
    .listing-items-col {      
      margin: 0 0 $padding-lg;      
      .listing-item {
        margin: 0 0 $padding-md; 
        padding: 10px; 
        @extend %transition-ease;
        @media (max-width: 991px) {
          background: $product-wrapper-bg;
          @include box-shadow(0 0 14px rgba($dark-color, 0.12));
        }
        .row {
          @include margin-start(0);
          @include margin-end(0);
        }      
        .product-image { 
          flex: 1;
          @include padding-start(0);
          @include padding-end(0); 
          max-width: 100px;  
          overflow: hidden;
          position: relative;
          &:before {
            content: '';
            display: block;
            @if $productImageRatioPercentage == 'auto' {
              padding-bottom: 100%;
            } 
            @else if $productImageRatioPercentage == false {
              padding-bottom: 0;
            } 
            @else {
              padding-bottom: $productImageRatioPercentage;
            }
          }
          img {
            @if $productImageAutoCrop == false {
              object-fit: contain;
              font-family:"object-fit: contain";
            } 
            @else {
              object-fit: cover;
              font-family:"object-fit: cover";
            }
            height: 100%;
          }
        } 
        .product-content {  
          @include padding-start($padding-inset-sm); 
          @include padding-end(0);
          flex: 1;
          position: relative;
          .product-price {
            line-height: 100%;
            .price {
              @media (max-width: 991px) {
                font-size: 16px;
              }
            }
            .price-compare {
              @media (max-width: 991px) {
                display: none;
              }
            }
          }
          .product-vendor {
            margin: 0 0 $padding-inset-xxs;
            font-size: 12px;
          }
          .product-content-inner {
            .product-name {              
              margin: 0 0 $padding-inset-xxs;
              line-height: 18px;
              color: $body-general-color;
              text-decoration: none;
              font-size: $product-font-size;
              font-family: $font-standard;
              @media (max-width: 991px) {
                font-size: 16px;
              }
            }            
          }  
        } 
        &:hover {
          @include box-shadow(0 0 14px rgba($dark-color, 0.12));
          background: $product-wrapper-bg;
        }
      }
      &.col-lg-6 {
        @media (max-width: 1199px) and (min-width: 992px) {
          .product-image {
            @include flex(0 0 110px);
            max-width: 110px; 
          }
          .product-content {               
            @include flex(0 0 calc(100% - 110px));
            max-width: calc(100% - 110px);
          }
        }
      }
    }
    .product-style-2 {
      .listing-items-col {
        .listing-item {
          >.row {
            @extend %flex-wrap;
          }
          .product-image {
            @media (max-width: 1199px) {
              max-width: 70px;
            }
          }
          .product-head {
            @include flex(0 0 80%);
            max-width: 80%;
            @extend %flexbox;
            @media (max-width: 991px) {
              @include flex(0 0 100%);
              max-width: 100%;
            }
            .product-content-inner {
              .product-name {
                @media (max-width: 1199px) and (min-width: 992px) {
                  font-size: 14px;
                }
              }
            }
          }
          .product__buttons-price {
            @include flex(0 0 20%);
            max-width: 20%;
            @media (max-width: 991px) {
              @include flex(0 0 100%);
              max-width: 100%;
            }
            .product-price {
              @extend %flexbox;
              flex-direction: column-reverse;
              align-items: flex-end;
              @media (max-width: 991px) {
                display: block;
              }
              @media (max-width: 1199px) and (min-width: 992px) {                
                .price-sale {
                  font-size: 14px;
                }
                .price-compare {
                  font-size: 12px;
                }
              }
            }
          }
        }
      }
    }
    .product-listing-items {
      .listing-title {                
        width: 100%;       
        margin-bottom: 20px; 
        margin-top: 15px;
        .prod-listing {                                     
          @include padding-end($padding-lg); 
          line-height: 100%;       
        }    
      }
      .owl-controls {
        .owl-nav {
          top: -46px;
        }
      }
      .row {
        .list-carousel {
          .title-hr {
            right: 75px;
          }
        }
      }
    }    
  }
.home-customer-review {
    .slider {
        max-width: 830px;
        margin: 0 auto;
        position: relative;
        overflow: hidden;
        > ul {
            > li {
                &:not(:first-child) {
                    display: none;
                }
            }
        }
        .bx-wrapper {
            margin: 0 auto 60px;        
            @media (max-width: 767px) {
                margin-bottom: 25px;
            }
        }
        .bxslider {      
            list-style: none;          
            > li {
                color: $body-general-color;
                font-size: $size-h4 - 2px;
                font-weight: 300;
                text-align: center;
                font-style: italic;
                width: 100% !important;
                line-height: 1.5;
            }
        }
    }
    .bx-controls-direction {
        display: none;
        > a {
            @include ab(50%, n, n, n);
            @include translate(0, -50%);
            width: 35px;
            height: 35px;
            line-height: 35px;
            color: var(--main-color);
            font-size: 0;
            text-align: center;            
            &:before {
                font-family: 'arenafont';
                font-size: $font-small-icon;
            }
            &.bx-prev {
                left: 0;
                &:before {
                    content: "\e82f";
                }
            }
            &.bx-next {
                right: 0;
                &:before {
                    content: "\e82c";
                }
            }
        }
    }
    .bx-pager {
        @extend %flexbox;
        @extend %justify-content-center;
        > a {
            text-align: center;     
            margin: 0 30px;            
            @include opacity(0.4);  
            @include flex(0 0 170px);
            max-width: 170px;       
            .author-img {
                display: block;
                height: 100px;
                width: 100px;
                padding: 5px;
                margin: 0 auto 20px;
                background: $light-color;
                @include box-shadow(0 0 21px rgba($dark-color, 0.1));
                @extend %border-radius-circle;                
                img {
                  display: inline;
                  @extend %border-radius-circle;
                }
            }
            .name {
                font-size: $body-font-size + 3px;
                display:block;
                padding: 0 0 10px;
            }
            .collector {
                display: block;
                font-weight: 300;
            }
            &.active {
                @include opacity(1);
            }
            &:hover {
                @media (min-width: 1025px) {
                    @include opacity(1);
                }
            }
        }
    }
}
.display-styles-1 {
    &.home-customer-review {
        @media (max-width: 767px) {            
            .bx-pager {
                display: block;
                >a {
                    @include flex(0 0 100%);
                    max-width: 100%;
                    display: block;
                    margin: 0 0 30px;
                }
            }
        }
    }
}
.display-styles-3, .display-styles-5 {
    &.home-customer-review {
        .slider-content {
            @include box-shadow(0 0px 12px -2px rgba($dark-color, 0.15));
            margin: 50px 0px 15px 0;
            padding: $padding-inset-md;
            .customer-image {
                width: 100px;
                margin: 0 auto;
                margin-top: -70px;
                padding-bottom: $padding-inset-md;
                img {
                    @extend %border-radius-circle;
                }
            }
            .customer-content {
                .customer-info {
                    >span {
                        display: block;
                        padding-bottom: $padding-sm;
                    }
                    .collector {
                        font-size: 12px;
                    }
                }
            }
            >p {
                margin: 0 0 $padding-inset-xs;
                font-style: italic;
                font-weight: 500;
            }
        }
    }
}
.display-styles-2, .display-styles-4 {
    &.home-customer-review {
        .slider-inner .slider-content {
            @include box-shadow(0 0px 12px -2px rgba($dark-color, 0.15));
            margin: 10px 0;
            padding: 30px 20px;
            >p {
                margin: 0 0 30px;
            }
        }
        .customer-content {
            @extend %flexbox;
            @extend %align-items-center;
            .customer-image {
                @include flex(0 0 20%);
                max-width: 100%;
                img {
                    @extend %border-radius-circle;
                }
            }
            .customer-info {
                @include flex(0 0 80%);
                max-width: 100%;
                @include padding-start($padding-inset-sm);
                span {
                    display: block;
                    padding: 3px 0;
                }
                .name {
                    font-weight: 500;
                }
                .collector {
                    font-size: $body-font-size - 2px;
                }
            }
        }
    }
}
