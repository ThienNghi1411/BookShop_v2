          @extend %flexbox;
        }
      }
      .currency-container {
        position: relative;
        >a {
          color: color-contrast($footer-copyright-bg);
          min-width: 98px;
          @extend %flexbox;
          .currency-code {
            @include padding-start($padding-inset-xs);
          }
          &:after {
            content: "\e80a";
            @extend %font-family;
            height: 20px;
            line-height: 20px;
            font-size: 13px;
            left: 0;
            position: absolute;
            transition: all .2s;
            @include rotate(90);
          }
        }
        .currencies {
          padding: 0;
          background: $body-second-color;
          @include ab(auto,0,38px,n);
          z-index: 999;
          width: 100%;
          display: none;
          li {
            a {
              height: 37px;
              line-height: 31px;
              color: #000;
              padding: 0 10px;
              @extend %flexbox;
              @extend %align-items-center;
              .currency-code {
                @include padding-start($padding-inset-xs);
              }
            }
          }
        }
        &.active {
          >a {
            &:after {
              @include rotate(180);
            }
          }
          .currencies {
            display: block;
          }
        }
      }
      .currency-flag {
        min-width: 30px;
        height: 20px;
      }
      .language-translator {
        @include margin-start($padding-inset-md);
        .stt-language {
          .country-selector {
            background: none;
          }
          .wgcurrent {
            border: 0;
            &:after {
              content: "\e80a"!important;
              @extend %font-family;
              background-image: none;
              height: 21px;
              line-height: 20px;
              left: 0;
              right: auto;
              color: color-contrast($footer-copyright-bg);
              @include rotate(90);
            }
            >a {
              height: 21px;
              line-height: 20px;
              color: color-contrast($footer-copyright-bg);
              padding-right: 0;
              @include padding-end($padding-lg);
            }
          }
          .wg-flags {
            a {
              &:before {
                margin-right: 0;
                @include margin-end($padding-inset-xs);
              }
            }
          }
        }
      }
    }
    @media (max-width: 767px) {
      display: block;
      text-align: center;
    }
  }
