.display-style-1 {
    .widget-partner {
      position: relative;
      background: $body-second-color;
      border: 1px solid $body-border-color;
      overflow: hidden;
      width: 100%;
      @extend %flexbox;
      @extend %flex-wrap;      
      > .row {
        margin: 0 -1px -1px;
        > div {
          padding: 10px;
          &.partner-item {
            @include border-inline-start(1px solid $body-border-color);
            border-bottom: 1px solid $body-border-color;
            @media (max-width: 767px) and (min-width: 576px) {
              @include flex(0 0 25%);
              max-width: 25%;
            }
            @media (max-width: 575px) {
              @include flex(0 0 33.333333%);
              max-width: 33.333333%;
            }
          }
        }
      }
      .partner-item {        
        &:hover {
          opacity: 0.7;
          @extend %transition-ease;
        }       
     }          
  }
  .owl-item {
    .partner-item {
      position: relative;
      &:before {
        content: '';
        height: 150%;
        width: 100%;
        background: $body-border-color;
        @include ab(0, n, n, -1px);
      }
    }
   }  
}
.display-style-2 {
  .title-wrapper {
    text-align: center;
    &:after {
      content: none;
    }
    .title {
      font-style: normal;
      padding: 0;
      &:after {
        content: none;
      }
    }
  }
  .widget-partner {
    > .partner-item {
      display: none;
    }
    .partner-item {
      margin-bottom: 30px;
      @media (max-width: 767px) {
        margin-bottom: 15px;
      }
      img {
        border: 3px solid transparent;
        @extend %transition-ease;
      }
    }   
  }
}
.blog-container {
    .blog-item {
        @extend %flexbox;           
        @media (max-width: 479px) {
            @include flex(0 0 100%);
            max-width: 100%;
        }
        .post-item-inner {
            height: auto;            
        } 
    }
    .view-all {
        margin: 30px auto 0;
        a {
            display: block;
            color: var(--main-color);
            border: 1px solid var(--main-color);
            font-size: $body-font-size + 1px;
            line-height: 30px;
            padding: 20px;
            text-transform: uppercase;
            text-align: center;
            &:hover {
                color: $body-title-color;
                border-color: $body-title-color;
            }
        }
    }
    .blog-list {
        .owl-item,
        .owl-stage {
            @extend %flexbox;        
        }
        .post-item-inner {
            .post-image {
                position: relative;
                &:before {
                  @extend %before;
              }
              @media (min-width: 1025px) {
                  &:hover {
                    &:before {
                        @include before-hover;
                        }
                    }
                }
            }
            &.date-on_image {
                .meta-data {
                    @include ab(0,0,n,n);
                    z-index: 1;
                    padding: $padding-xs;
                    margin-top: $padding-inset-sm;
                    background: var(--main-color)-text;
                    color: color-contrast(var(--main-color)-text);
                    line-height: 1;
                }
            }
            .post-content {
                .info {
                    a[class="text-"] {
                        color: red;
                        &:after {
                            content: "\e82c";
                            @include font-family;
                        }
                    }
                }
            }
        }  
        .owl-controls .owl-nav > div {
            &.owl-prev {
                @media (min-width: 1240px) {
                    left: -25px;
                }
            }
            &.owl-next {
                @media (min-width: 1024px) {
                    right: -25px;
                }
            }
        } 
    }
}
.blog-content {
    .post-item-inner {      
        .link-to-post {
            display: block;
            margin-top: auto;
        }
    } 
    &.no-padding  {
        .row {
            margin: 0;
            > div {
                padding: 0;
            }
        }
    }
}
.border-radius {
    .blog-container {
        .post-item-inner .post-image {
            @include border-radius(10px, 10px, 10px, 10px);
        }
    }
}
.product-playlist-section {
	position: relative;
	overflow: hidden;
	.view-all.d-lg-none.d-xl-none {
		padding-top: 20px;
	}
	.ppl-wrapper {
		@extend %flexbox;
		height: 650px;
		overflow: hidden;
		@include scroll-bar-ul;
		@media (max-width: 1199px) {
			height: 490px;
		}
		@media (max-width: 767px) {
			height: 100%;
			display: block;
		}
		>div {
			@include flex(0 0 50%);
			max-width: 50%;
			@media (max-width: 767px) {
				@include flex(0 0 100%);
				max-width: 100%;
				width: 100%;
			}
		}
	}
	.product-big-image {
		position: relative;
		.product-big-image__image {
			height: 100%;
			.product-wrapper {
				height: 100%;
				margin: 0;
				.product-head {
					height: 100%;
					.product-image {
						height: 100%;
					}
				}
				.featured-img {
					height: 100%;
				}
			}
		}
		.inlinePlayButton {
			font-size: 32px;
			width: 50px;
			height: 50px;
			line-height: 50px;
			font-size: 0;
			&:before {
				content: '\e86a';
				@include font-family;
				font-size: 30px;
				@include margin-start(4px);
			}
		}
		.audio-wrapper {
			width: 100%;
			height: 70px;
			position: fixed;
			bottom: -15px;
			z-index: 1000;
			left: 0;
			right: 0;
			padding: 0 15px;		
			border-top: solid 1px #6d6a73;
			border-bottom: 1px solid transparent;
			display: none;
			@include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
			@extend %justify-content-center;
			@extend %align-items-center;
			>audio {
				width: 100%;
				max-width: 900px;
				height: 50px;
				@media (max-width: 1199px) {
					max-width: 700px;
				}
			}
			&.played {
				bottom: 0;
				z-index: 9991;
				@extend %flexbox;
			}
		}
		.video-wrapper {
			position: absolute!important;
			width: 100%!important;
			height: 100%!important;
			top: 0;
			overflow: hidden;
			.playerBox {
				position: absolute;
				top: 0;
				left: 0;     
				min-width: 100%;
				min-height: 100%;
				width: auto!important;
				height: auto!important;
				z-index: 3;
				margin: 0!important;
			}
		}
	}
	.product-playlist {
		overflow-y: auto;
		.product-wrapper {
			padding: $padding-inset-xs;
			@extend %flexbox;
			@extend %flex-wrap;
			background: none;
			@include transition(0.25s all ease-in-out);
			border-bottom: 1px solid rgba($body-border-color, 0.2);
			margin: 0;
			border-radius: unset!important;
			.product-head {
				@include flex(0 0 70%);
				max-width: 70%;
				@extend %flexbox;
				@extend %flex-wrap;
				@extend %align-items-center;
				@media (max-width: 991px) {
					@include flex(0 0 100%);
					max-width: 100%;
				}
				.product__number {
					padding: 10px 5px 10px 15px;
					font-size: 16px;
				}
				.product-image {
					width: 100px;
					@media (max-width: 1199px) {
						width: 70px;
					}
					.btn-play-pause {
						@include ab(0,n,n,n);
						width: 100%;
						height: 100%;
						background: none;
						opacity: 0;
						border: 0;
						@media (max-width: 1024px) {
							opacity: 1;
						}
						i {
							padding: 3px 5px;
							font-size: $font-icon;
							background: $button-btn1;
							color: color-contrast($button-btn1);
							@extend %border-radius-circle;
							&:before {
								font-weight: 900;
								margin: 0;
							}
						}
					}
				}
				.product-content {
					flex: 1;
					padding: 0;
					@include padding-start($padding-inset-sm);
					.product-name {
						@media (max-width: 1199px) {
						font-size: 14px;	
						}
						a {
							white-space: normal;
						}
					}
					.product-vendor {
						margin-bottom: 8px;
						font-size: $size-h6;
						@media (max-width: 1199px) {
							font-size: 10px;
						}
					}
				}
			}
			.product__buttons-price {
				@include flex(0 0 30%);
				max-width: 30%;
				padding-top: $padding-xs;
				@media (max-width: 1199px) {
					padding-top: 0;
				}
				@media (max-width: 991px) {
					@include flex(0 0 100%);
					max-width: 100%;
				}
				.product-price {
					@extend %flexbox;
					@extend %flex-wrap;
					flex-direction: row-reverse;
					align-items: center;
					@media (max-width: 991px) {
						justify-content: flex-end;
					}
					.price-compare {
						@include padding-start(0);
						@include padding-end($padding-inset-xs);
						@media (max-width: 1199px) and (min-width: 992px) {
							@include padding-end(0);
						}
					}
					.price-sale {
						@media (max-width: 1199px) {
							font-size: 14px;
						}
					}
				}
				.product__buttons {
					.product-button__sharing {
						>i {
							display: block;
						}
					}
				}
			}
			&:hover {
				background: rgba($product-wrapper-bg, 0.2);
				@include box-shadow(0 0 18px rgba($product-wrapper-bg, 0.15)); 
				.product-head .product-image .btn-play-pause {
					opacity: 1;
				}
			}
		}
	}
	.view-all {
		a {
			border-bottom: 1px solid currentColor;
		}
	}
}

#topbar {
	position: relative;
	.topbar {
		&.image {			
			@include ab(50%,0,n,0);
			width: 100%;
			z-index: 99;
			margin-top: -118px;
			@media (max-width: 767px) {
				top: 0;
				margin-top: 0;
			}
		}
		.table-row {
			padding: 10px 0;
		}
		.custom-text {
			@extend %flexbox;
			color: $topbar-text-color;
			font-weight: $top-bar-weight;
			.custom-icon {
				@include padding-end($padding-inset-xs);
			}
			p {
				margin: 0;
			}
		}
		.text-center, .text-lg-center {
			.custom-text {				
				@extend %justify-content-center;
			}
		}
		.text-left, .text-lg-left {
			.custom-text {				
				@include justify-content(flex-start);
			}
		}
		.text-right, .text-lg-right {
			.custom-text {				
				@include justify-content(flex-end);
			}
		}
		.block-countdown {
			@extend %countdown-product;
			padding: 0 !important;
			min-height: 70px;
			@media (max-width: 575px) {
				.wrapper-countdown {
					margin: 5px 0;
				}
				.countdown-period {
					padding: 5px !important;
				}
			}
		}
		.btn-topbar {
			padding: 9px 20px;
			text-transform: capitalize;
			line-height: 20px;
			@media (max-width: 575px) {
				padding: 5px 20px;
				font-size: 12px;
			}
		}
	}
}
/* Page 404 */
.template404 {
  #footer-content {
    &.display-type-1 {
      padding-top: 70px;
    }
  }
  #main-content {
    padding: 0;
  }
  #widget-newsletter {
    display: none;
  }
}
.page-404 {  
  overflow: hidden;
  padding-bottom: 70px;
  .box-404-text {
    padding: 10% 0 12%;
    h1 {
      font-size: 250px;
      line-height: 100%;
      margin: 0 auto 5%;
      font-weight: 900;   
      color: var(--main-color);   
      letter-spacing: 20px;
      @media (max-width: 767px) {
        font-size: 30vw;
        letter-spacing: 5px;
        margin-bottom: 40px;
      }
    }
    .h1 {
      font-weight: 600;
      margin-bottom: 3%;
      color: $body-general-color;
      @media (max-width: 767px) {
        font-size: 26px;
        margin-bottom: 30px;
      }
    }
    .btn {
     padding: 15px 65px;
     font-size: $size-h4; 
     background-size: 800px;
     @media (max-width: 479px) {
       padding-left: 10px;
       padding-right: 10px;
       width: 100%;
     }
    }
  }
  .image-404 {
    width: 100vw;    
    position: relative;
    right: 50%;
    margin-right: -50vw;
  }
}

.section-separator {
  overflow: hidden;
  &.color,
  &.image {
    padding: 25px 0;
  }
  &.image.lazyloaded {
    background-size: 100%;
  }
  &.section-separator-margin-top {
    padding-top: 30px;
    @media (max-width: 767px) {
      padding-top: 15px;
    }
  }
  &.section-separator-margin-bottom {
    padding-bottom: 30px;
    @media (max-width: 767px) {
      padding-bottom: 15px;
    }
  }
}
.separator-subtext {
  display: block;
  padding-top: 20px;
}
.separator-heading {
  &.text-left, &.text-center, &.text-right {
    margin: 0 auto;
    padding: 0 $padding-container;
    @media (min-width: 576px) {
      max-width: 540px;
    }
    @media (min-width: 768px) {
      max-width: 720px;
    }
    @media (min-width: 992px) {
      max-width: 960px;
    }
    @media (min-width: 1200px) {
      max-width: 1140px;
    }
    @media (min-width: 1440px) {      
      max-width: $width-container; 
    }
  }
}
.separator-heading-line { 
  max-width: $width-container;
  padding: 0 $padding-container;
  margin: 0 auto 37px;
  text-align: center;
  h2 {
    margin: 0 auto 1.5%;
    font-family: $font-heading;
    &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
      line-height: 1.0;
      font-weight: 700;
    }
    &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
      font-weight: 100;
    }
  }
  .separator-subtext {
    display: block;
    font-family: $font-heading;
    &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
      line-height: 1.0;
      font-weight: 700;
    }
    &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
      font-weight: 100;
    }
  }
  .separator-sub-line {
    max-width: 100%; /*20%;*/
    margin: 0 auto 2%;
    overflow: hidden;
    border-top-width: 1px;
    border-bottom-width: 0;
    border-left-width: 0;
    border-right-width: 0;
    &.sub-line-bt {
      margin: 2% auto 0;
    }
    &.sub-double-line {
      max-width: 80px;
      border-width: 1px;
    }
  }
  &.side-line {
    h2 {
      position: relative;
      margin: 0 auto;
      display: inline-block;
      padding: 0 50px;
      @media (max-width: 479px) {
        padding: 0 20px;
      }
      span {
        display: inline-block;
        padding: 0 20px;
        position: relative;
        z-index: 1;
        white-space: nowrap;
        background: $body-background-color;
      }
      &:after,
      &:before {
        content: '';
        height: 1px;
        width: 50%;
        border-top-width: 1px;
        border-bottom-width: 0;
        border-left-width: 0;
        border-right-width: 0;
        @include ab(50%, n, n, n);
        @include translate(0, -50%);
      }
      &:before {
        left: 50%;
      }
      &:after {
        right: 50%;
      }
    }
    &.line-solid {
      h2 {
        &:after,
        &:before {
          border-style: solid;
        }
      }
    }
    &.line-dotted {
      h2 {
        &:after,
        &:before {
          border-style: dotted;
        }
      }
    }
    &.line-dashed {
      h2 {
        &:after,
        &:before {
          border-style: dashed;
        }
      }
    }
    .separator-logo {
      margin: 8px auto 13px;
      display: block;
    }
    .separator-subtext {
      margin: 8px auto 0;
    }
  }
  &.left-line {
    text-align: start;
    h2 {
      position: relative;
      padding: 0;
      display: block;
      &:after {
        content: none !important;
      }
      &:before {
        content: '';
        height: 1px;
        width: 100%;
        border-width: 1px;
        @include ab(50%, n, n, 0);
        @include translate(0, -50%);
      }
      span {
        display: inline-block;
        padding: 0 8% 0 0;
        position: relative;
        background: $body-background-color;
      }
    }
    &.line-solid {
      h2 {
        &:before {
          border-style: solid;
        }
      }
    }
    &.line-dotted {
      h2 {
        &:before {
          border-style: dotted;
        }
      }
    }
    &.line-dashed {
      h2 {
        &:before {
          border-style: dashed;
        }
      }
    }
  }
  &.right-line {
    text-align: end;
    h2 {
      position: relative;
      padding: 0;
      display: block;
      &:after {
        content: none !important;
      }
      &:before {
        content: '';
        height: 1px;
        width: 100%;
        border-width: 1px;
        @include ab(50%, 0, n, n);
        @include translate(0, -50%);
      }
      span {
        display: inline-block;
        padding: 0 0 0 8%;
        position: relative;
        background: $body-background-color;
      }
    }
    &.line-solid {
      h2 {
        &:before {
          border-style: solid;
        }
      }
    }
    &.line-dotted {
      h2 {
        &:before {
          border-style: dotted;
        }
      }
    }
    &.line-dashed {
      h2 {
        &:before {
          border-style: dashed;
        }
      }
    }
  }
  &.line-solid {
    .separator-sub-line {
      border-style: solid;
    }
  }
  &.line-dotted {
    .separator-sub-line {
      border-style: dotted;
    }
  }
  &.line-dashed {
    .separator-sub-line {
      border-style: dashed;
    }
  }
}
  /* footer --------------------------*/
  #footer-content {           
    @extend %flexbox;
    @extend %flex-diretion-column;
    &.display-type-1 {  
      position: relative;
      &[data-bg-type="image"] {
       .newsletter-social-media-inner {
         background-image: url({{ settings.ft_bg_image | img_url: '1920x', format: 'pjpg' }});
         background-size: cover;
       }
     }
