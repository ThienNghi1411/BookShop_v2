%btn-cart {
  min-width: 40px;
  line-height: 28px;
  padding: 10px 33px;                           
  white-space: nowrap; 
  text-align: center;
  display: block;   
}
%animation-fa-spin {
  -webkit-animation: fa-spin 2s infinite linear;
    animation: fa-spin 2s infinite linear;
}
%filter-before {
  content: "";
  @include ab(3px, 0, n, n);
  width: 16px;
  height: 16px;
  background: transparent;
  border: 1px solid $body-border-color;
  @extend %border-radius-2;
  @extend %transition-ease;
}
%filter-after {
  content: "\e805";
  font-family: "arenafont";
  font-size: 12px;
  width: 16px;
  height: 16px;
  line-height: 16px;
  text-align: center;
  bottom: auto;
