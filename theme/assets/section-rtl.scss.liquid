}
@mixin ab($t, $l, $b, $r) {
  position: absolute;
  display: block;
  @if $l != n { right: $l; }
  @if $t != n { top: $t; }
  @if $r != n { left: $r; }
  @if $b != n { bottom: $b; }
}
@mixin rotate ($deg) {
  @include transform(rotate(#{$deg}deg));
}
