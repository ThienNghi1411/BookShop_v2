      margin: 0;
    }
    .row {
      margin: 0;
      > div {
        padding: 0;          
      }
    }
    .banner-item {
      margin-bottom: 0;
      padding: 0 !important;
      @media (max-width: 767px) {
        margin-bottom: 15px;
      }
    }
  }
  .home-banner-masonry {
    .text-box-overlay-1 {
      .text {
        @media (min-width: 768px) and (max-width: 1199px) {
          padding: 10px 5px 8px;
        }
        @media (max-width: 767px) {
          padding: 4px 5px;
        }
        .title {
          @media (min-width: 768px) and (max-width: 1199px) {
            font-size: 18px;
          }
        }
      }
    }
  }
  &.has-padding {
    .home-banner-masonry {      
      .bc-masonry {
        margin: 0 -15px;
        @media (max-width: 575px) {
          margin: 0 -7.5px;
        }
        .banner-item {
          padding: 0 15px;
          @media (max-width: 575px) {
            padding: 0 7.5px;
          }
        }
      }
    }
  }
  .home-banner-masonry {
    .text {
      @media (min-width: 768px) and (max-width: 1199px) {
        .title {
          font-size: 20px;
        }
      }
    }
  }
  .vertical-menu {
    margin-bottom: 30px;
    @media (max-width: 767px) {
      margin-bottom: 15px;
    }    
    .sidemenu-holder .navigation {
      padding-top: 0;
      .navbar {
        z-index: 9;
        @media (max-width: 991px) {
          position: relative;
        }
        .view-all {
          a {
            color: var(--main-color);
          }
          i {
            @include margin-start(-3px);
            font-size: $font-small-icon;
          }
        }
        .main-nav {          
          .dropdown {
            .expand {
              left: 4px;
              font-size: inherit;
              width: 40px;
              &.active {
                &:before {
                  content: '\e80a';
                  @include font-family;
                }
              }
            }
            .dropdown-menu {
              position: static;
              display: none!important;
              visibility: visible!important;
              opacity: 1!important;
              transform: translate(0,0) !important;
              padding: 0;
              li {
                padding: 5px 10px 5px 0;
                a {
                  font-size: inherit;
                }
                .expand {
                  left: 4px;
                  font-size: inherit;
                }
                &:not(:last-child) {
                  border-bottom: 1px solid $body-border-color;
                }
              }
              &:before {
                display: none;
              }
            }
            &.active {
              >.dropdown-menu {
                position: static;
                display: block!important;
                box-shadow: unset;
                margin: 0;
                width: 100%;
              }
            }
          }
        }
      }
    }
  }
  .view-all.text-center {
    @media (max-width: 767px) {      
      width: 100%;
      a {
        border-bottom: 1px solid var(--main-color);
        font-size: 16px;
        color: var(--main-color);
      }
    }
  }
  .home-banner-items {
    @media (max-width: 575px) {
      .row {
        @include margin-start(-7.5px);
        @include margin-end(-7.5px);
        >div {
          @include padding-start(7.5px);
          @include padding-end(7.5px);
        }
      }
    }
  }
  .home-banner-carousel {
    > .banner-item {
      display: none;
    }
    .banner-item {
      margin-bottom: 0;
    }
  }    
  &.effect-2 {    
    .banner-item {
      .image {
        a {           
          &:before {
            @extend %before;
          }
        }
        &:hover {
          @media (min-width: 1025px) {
            a {
              &:before {
                @include before-hover;
              }
            }
          }          
        }
      }
    }    
  }
  /*Effect Zoom Fade*/
  &.effect-3 {
    .banner-item {
      .image {
        img {
          @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
          opacity: 1;
        }
      }
      .text-box-default {
        @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
        margin-bottom: 3px;
      }
      &:hover {
        .image {
          img {
            opacity: 0.7;
            @include scale(1.1, 1.1);
          }
        }
        .text-box-default {
          @include box-shadow(1px 0px 4px rgba($overlay-color, 0.1));
        }
      }
    }
  }
  /*End Zoom Fade*/

  /*Effect Blur*/
  &.effect-4 {
    .banner-item {
      &:hover {            
        .image {
          img {
            @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
            -webkit-filter: blur(5px);
            filter: blur(5px);
          }
        }
      }
    }
  }
  /*End Blur*/

  /*Effect Glow*/
  &.effect-5 {
    .banner-item {
      &:hover {            
        .image {
          img {
            @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
            -webkit-filter: brightness(1.1);
            filter: brightness(1.1);
          }
        }
      }
    }
  }
  /*End Effect Glow*/

  /*Effect Color*/
  &.effect-6 {
    .banner-item { 
    @media(min-width: 1025px) {      
      .image {
        img {
          @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
          -webkit-filter: grayscale(100%);
          filter: grayscale(100%);
        }
      }
      &:hover {
        .image {
          img {
            -webkit-filter: grayscale(0);
            filter: grayscale(0);              }
          }
        }
    }          
      }
    }
    /*End Effect Color*/

    /*Effect Grayscale*/
    &.effect-7 {
      .banner-item {
        &:hover {
          .image {
            img {
              @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
              -webkit-filter: grayscale(100%);
              filter: grayscale(100%);
            }
          }
        }
      }
    }
    /*End Grayscale*/
  &.layout-full {
    padding: 0;
  }         
}
