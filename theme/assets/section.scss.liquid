  .product-listing-section {
    .listing-items-col {      
      margin: 0 0 $padding-lg;      
      .listing-item {
        margin: 0 0 $padding-md; 
        padding: 10px; 
        @extend %transition-ease;
        @media (max-width: 991px) {
          background: $product-wrapper-bg;
          @include box-shadow(0 0 14px rgba($dark-color, 0.12));
        }
        .row {
          @include margin-start(0);
          @include margin-end(0);
        }      
        .product-image { 
          flex: 1;
          @include padding-start(0);
          @include padding-end(0); 
          max-width: 100px;  
          overflow: hidden;
          position: relative;
          &:before {
            content: '';
            display: block;
            @if $productImageRatioPercentage == 'auto' {
              padding-bottom: 100%;
            } 
            @else if $productImageRatioPercentage == false {
              padding-bottom: 0;
            } 
            @else {
              padding-bottom: $productImageRatioPercentage;
            }
          }
          img {
            @if $productImageAutoCrop == false {
              object-fit: contain;
              font-family:"object-fit: contain";
            } 
            @else {
              object-fit: cover;
              font-family:"object-fit: cover";
            }
            height: 100%;
          }
        } 
        .product-content {  
          @include padding-start($padding-inset-sm); 
          @include padding-end(0);
          flex: 1;
          position: relative;
          .product-price {
            line-height: 100%;
            .price {
              @media (max-width: 991px) {
                font-size: 16px;
              }
            }
            .price-compare {
              @media (max-width: 991px) {
                display: none;
              }
            }
          }
          .product-vendor {
            margin: 0 0 $padding-inset-xxs;
            font-size: 12px;
          }
          .product-content-inner {
            .product-name {              
              margin: 0 0 $padding-inset-xxs;
              line-height: 18px;
              color: $body-general-color;
              text-decoration: none;
              font-size: $product-font-size;
              font-family: $font-standard;
              @media (max-width: 991px) {
                font-size: 16px;
              }
            }            
          }  
        } 
        &:hover {
          @include box-shadow(0 0 14px rgba($dark-color, 0.12));
          background: $product-wrapper-bg;
        }
      }
      &.col-lg-6 {
        @media (max-width: 1199px) and (min-width: 992px) {
          .product-image {
            @include flex(0 0 110px);
            max-width: 110px; 
          }
          .product-content {               
            @include flex(0 0 calc(100% - 110px));
            max-width: calc(100% - 110px);
          }
        }
      }
    }
    .product-style-2 {
      .listing-items-col {
        .listing-item {
          >.row {
            @extend %flex-wrap;
          }
          .product-image {
            @media (max-width: 1199px) {
              max-width: 70px;
            }
          }
          .product-head {
            @include flex(0 0 80%);
            max-width: 80%;
            @extend %flexbox;
            @media (max-width: 991px) {
              @include flex(0 0 100%);
              max-width: 100%;
            }
            .product-content-inner {
              .product-name {
                @media (max-width: 1199px) and (min-width: 992px) {
                  font-size: 14px;
                }
              }
            }
          }
          .product__buttons-price {
            @include flex(0 0 20%);
            max-width: 20%;
            @media (max-width: 991px) {
              @include flex(0 0 100%);
              max-width: 100%;
            }
            .product-price {
              @extend %flexbox;
              flex-direction: column-reverse;
              align-items: flex-end;
              @media (max-width: 991px) {
                display: block;
              }
              @media (max-width: 1199px) and (min-width: 992px) {                
                .price-sale {
                  font-size: 14px;
                }
                .price-compare {
                  font-size: 12px;
                }
              }
            }
          }
        }
      }
    }
    .product-listing-items {
      .listing-title {                
        width: 100%;       
        margin-bottom: 20px; 
        margin-top: 15px;
        .prod-listing {                                     
          @include padding-end($padding-lg); 
          line-height: 100%;       
        }    
      }
      .owl-controls {
        .owl-nav {
          top: -46px;
        }
      }
      .row {
        .list-carousel {
          .title-hr {
            right: 75px;
          }
        }
      }
    }    
  }

/*Customize Block*/
.custom-content-width {
  overflow: hidden;
  cursor: pointer;
  .col-md-12 {
    padding: 0;
  }
}
.custom-content-wrapper {
  &.custom-content-margin-top {
    margin-top: 30px;
  }
  &.custom-content-margin-bottom {
    margin-bottom: 30px
  }
  &.only-min-height { 
    .custom-item {
      &.col-md-12 {
        position: relative;
        @media(max-width: 767px) {
          min-height: auto!important;
        }
        .custom-item-inner {
          width: 100%;
          padding: 0;
          &.custom-item-inner-text {
            .custom-block-heading-text {
              margin: 0 auto;
              padding-bottom: 35px;
              text-align: center;
            }
          }
        }
        &.align-center {
          .custom-item-inner {
            text-align: center;
            padding: 0 10px;
          }
        }
        &.align-left {
          .custom-item-inner {
            text-align: start;
          }
        }
        &.align-right {
          .custom-item-inner {
            text-align: end;
          }
        }
      }
    }
  }
}
.custom-content-header {
  margin: 0 auto 3%;
}
.custom-item-inner-collection,
.custom-item-inner-html {
  display: block;
}
.custom-block-heading-text,
.custom-block-body-text {
  margin-left: auto;
  margin-right: auto;
  font-weight: normal;
}
.custom-block-heading-text {
  font-weight: 700;
}
.custom-block-body-text {   
  p {
    margin: 0;
    line-height: 24px;
  }
}
.custom-content {  
  width: 100%;  
  width: auto;
  padding: 0;
  @extend %flexbox;
  @include align-items(stretch);
  @extend %flex-wrap;
  .info {
    padding: 15px 20px;
  }
  .custom-block-heading-text {
    margin: 0 auto 3%;
  }
}
.custom-item {  
  @include flex(0 0 auto);
  margin: 0 0 30px;
}
.custom-item-inner {
  position: relative;
  max-width: 100%;
  &.custom-item-inner-bgimage {
    max-width: 1170px;
    margin: 0 auto;
    padding: 0 4%;
    .custom-block-heading-text {
      max-width: 100%;
    }
  }
  .custom-block-image {
    margin-top: -76px;
  }
}
.custom-item-inner-product,
.custom-item-inner-collection {  
  .info {
    > a {
      @extend .h5;
      text-transform: uppercase;
      font-family: $font-heading;      
      @media (max-width: 479px) {        
        text-transform: none;
      }
    }
    .product-author {
      color: $body-light-color;
      font-size: $body-font-size - 3px;
      text-transform: uppercase;
      padding: 0 0 5px;
      font-weight: 500;
    }
    .product-name {
      color: $body-general-color;
      font-family: $font-standard;
      margin: 0 0 5px;      
      font-weight: 600;
    }
  }
}
.align-center,
.align-bottom {
  text-align: center;
}
.align-top {  
  @include align-items(flex-start);
  @include align-self(flex-start);
}
.align-center {
  @extend %align-items-center;
  @include align-self(center);
}
.align-bottom {
  @include align-items(flex-end);
  @include align-self(flex-end);
}
.custom-item-inner-video {  
  .home-video {       
    overflow: hidden;
    position: relative;
    .image-layer {
      position: relative; 
      &:before {
        content: '';
        @include ab(0, 0, 0, 0);
        // background: rgba($main-color, 0.8);
        z-index: 1;
      }  
      @media (max-width: 767px) {
        img {
          @include ab(0, 0, n, n);
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
      }
      .button-text-wrapper {
        @include ab(50%, 50%, n, n);
        @include translate(-50%, -50%);        
        z-index: 2;
        color: color-contrast($main-color);
        text-align: center;
        > div {
          &:not(:first-child) {
            margin-top: 20px;
            @media (max-width: 575px) {
              margin-top: 0;
            }
          }
        }
        .text {
          font-size: $size-h1 + 4px;
          line-height: 1.5;

          @media (max-width: 767px) {
            font-size: 5vw;
          }
        }
        .b-play, .b-pause {
          border: none;
          background: none;
          padding: 0;
          color: inherit;
        }
        .static-control,
        .b-play, .b-pause {
          i {
            font-size: 70px;
            line-height: 100%;
            @media (max-width: 575px) {
              font-size: 50px;
            }
          }
        }
      }
    }
    .video {
      @extend %transition-ease;
      @include ab(50%, 50%, n, n);
      @include translate(-50%, -50%);
      min-width: 100%;
      min-height: 100%;
      z-index: -9;
      visibility: hidden;
    }
    &.is_play {
      .video {
        z-index: 3;
        visibility: visible;
      }
      .image-layer .button-text-wrapper {
        z-index: 4;
        .text {
          display: none;
        }
      }
    }
  }
}




