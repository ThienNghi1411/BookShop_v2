%countdown-product {
  .wrapper-countdown {
    margin: $padding-xs 0;
    > div{
      margin: 0;
      padding: 0;
      background: none;
      border: none;
    }
    .countdown-row {
      display: inline-block;
      text-align: start;
      width: auto;
      padding: 0;
      .countdown-section {
        width: 65px;
        color: color-contrast($main-color);
        &:not(:last-child) {
          @include margin-end($padding-inset-sm);
          @media (max-width: 767px) {
            @include margin-end($padding-inset-xs);
          }
        }
        @media (max-width: 767px) {
          width: 55px;
        }
        .countdown-period {
          background: rgba($main-color, 0.8);
          padding: 8px 5px;
          @include border-radius(0, 0, 5px, 5px);
        }
      }
      .countdown-amount {
        position: relative;   
        font-weight: 700;  
        display: block;
        font-size: $size-h3;
        background: $main-color;
        padding: 8px 5px;
        @include border-radius(5px, 5px, 0, 0);
        @media (max-width: 767px) {
          font-size: $size-h3 - 4px;
        }
      }
    }
  }
}
%quantity-product {
  text-align: center;
  position: relative; 
  padding: 3px 54px;          
  input[type="number"] {
    -moz-appearance: textfield;
    -webkit-appearance: textfield;            
  }
  input[type=number]::-webkit-inner-spin-button,
  input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;    
  } 
  input {
    width: 60px;
    height: 44px;
    padding: 9px 5px;
    text-align: center;    
    background-repeat: no-repeat;
    background-attachment: fixed;
    background: transparent;    
    border: 2px solid #5e5e5e;                                                
  }
  .qty-inner > span {
    height: 42px;
    line-height: 38px;
    width: 42px;
    cursor: pointer;
    font-size: $body-font-size + 2px;     
    border: 2px solid $body-border-color;
    @extend %border-radius-circle;            
    @extend %transition-ease;
    @include ab(3px, n, n, n);            
    &.qty-down {                                          
      left: 0;
    }
    &.qty-up {
      right: 0;
    }
    i {
      color: inherit;
    }            
    &:hover {
      border-color: $main-color;
      color: $main-color;
    }            
  }
}
%anmation-duration {
  -webkit-animation-duration: .5s;
  animation-duration: .5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@mixin pc-header-fixed {
  position: fixed;
  z-index: 9999;
  top: 0;
  left: 0;
  right: 0;
  -webkit-animation-name: fadeInDown;
  animation-name: fadeInDown;  
}
@mixin effect-fade-in_out {
  content: "";
  @include ab(n,0,0,0);
  width: 100%;
  height: 100%;
  @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
  background: rgba($overlay-color, 0.25);
  z-index: 1;
}
@mixin scroll-bar-ul { 
  scrollbar-color: $main-color $body-second-color;
  scrollbar-width: thin; 
  .product-playlist::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  >div::-webkit-scrollbar-thumb {
    background-color: $main-color;
  }
}
@mixin scroll-bar-search { 
  scrollbar-color: $main-color $body-second-color;
  scrollbar-width: thin; 
  ul::-webkit-scrollbar-track
  {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar {
    width: 6px;
    background-color: $body-second-color;
  }
  ul::-webkit-scrollbar-thumb {
    background-color: $main-color;
  }
}
.banner-item {     
  margin: 0 auto 30px;
  @media (max-width: 767px) {
    margin-bottom: 15px;
  }
  .image {
    background: $background-pre-load;
  }
  .text-box-default {
    .text {
      padding: $padding-inset-md $padding-inset-sm;
    }
  }
  .text-box-overlay-1 {
    .text {
      position: absolute;
      bottom: 0;
      display: block;
      width: 100%;
      padding: 15px 10px;
      .title {
        display: inline-block;
      }      
      .subtitle {
        display: inline-block;
        margin: 0;
        line-height: inherit;
        float: right;
        @media (max-width: 991px) {
          float: none;
        }
        @media (max-width: 767px) and (min-width: 350px){
          @include padding-start($padding-inset-xs);
        }
      }
    }
  }
  .text-box-overlay-2 {
    .text {      
      padding: 25px 30px; 
      display: table;
      width: -moz-fit-content;            
      width: fit-content;
      pointer-events: none;
      @media (max-width: 991px) and (min-width: 768px) {
        padding: 10px;
      }
      @media (max-width: 479px) {
        padding: 5%;
      }      
    }
  }
  .banner-item-inner {
    position: relative;
    overflow: hidden;    
  }    
  .title {
    line-height: 100%;
    font-weight: 400;
  }
  .subtitle {                   
    font-weight: 400;
    margin-top: 10px;
    line-height: 100%;
  }
  .offer {
    margin-top: 10px;
    font-weight: 700;
    .offer-text {
      padding: 0;
      background: none;
      &:before {
        content: none;
      }
    }
  } 
  .btn-group {  
    display: block;  
    margin-top: 10px;  
    .btn {
      padding: 5px 30px;     
      position: relative;
      display: inline-block;     
      @extend %border-radius-30;
      @extend %transition-ease; 
      @media (max-width: 991px) {
        display: none;
      }     
      span {          
        position: relative;
        z-index: 1;
        display: block;
        white-space: nowrap;
        overflow: hidden;
        text-transform: initial;
        text-overflow: ellipsis;
      }
    }
  } 
}
.home-banner {        
  &.padding-item-false {
    .home-banner-masonry {
      margin: 0;
    }
    .row {
      margin: 0;
      > div {
        padding: 0;          
      }
    }
    .banner-item {
      margin-bottom: 0;
      padding: 0 !important;
      @media (max-width: 767px) {
        margin-bottom: 15px;
      }
    }
  }
  .home-banner-masonry {
    .text-box-overlay-1 {
      .text {
        @media (min-width: 768px) and (max-width: 1199px) {
          padding: 10px 5px 8px;
        }
        @media (max-width: 767px) {
          padding: 4px 5px;
        }
        .title {
          @media (min-width: 768px) and (max-width: 1199px) {
            font-size: 18px;
          }
        }
      }
    }
  }
  &.has-padding {
    .home-banner-masonry {      
      .bc-masonry {
        margin: 0 -15px;
        @media (max-width: 575px) {
          margin: 0 -7.5px;
        }
        .banner-item {
          padding: 0 15px;
          @media (max-width: 575px) {
            padding: 0 7.5px;
          }
        }
      }
    }
  }
  .home-banner-masonry {
    .text {
      @media (min-width: 768px) and (max-width: 1199px) {
        .title {
          font-size: 20px;
        }
      }
    }
  }
  .vertical-menu {
    margin-bottom: 30px;
    @media (max-width: 767px) {
      margin-bottom: 15px;
    }    
    .sidemenu-holder .navigation {
      padding-top: 0;
      .navbar {
        z-index: 9;
        @media (max-width: 991px) {
          position: relative;
        }
        .view-all {
          a {
            color: $main-color;
          }
          i {
            @include margin-start(-3px);
            font-size: $font-small-icon;
          }
        } 
        .main-nav {          
          .dropdown {
            .expand {
              right: 4px;
              font-size: inherit;
              width: 40px;
              &.active {
                &:before {
                  content: '\e80a';
                  @include font-family;
                }
              }
            }
            .dropdown-menu {
              position: static;
              display: none!important;
              visibility: visible!important;
              opacity: 1!important;
              transform: translate(0,0) !important;
              padding: 0;
              li {
                padding: 5px 0 5px 10px;
                a {
                  font-size: inherit;
                }
                .expand {
                  right: 4px;
                  font-size: inherit;
                }
                &:not(:last-child) {
                  border-bottom: 1px solid $body-border-color;
                }
              }
              &:before {
                display: none;
              }
            }
            &.active {
              >.dropdown-menu {
                position: static;
                display: block!important;
                box-shadow: unset;
                margin: 0;
                width: 100%;
              }
            }
          }
        }      
      }
    }
  }
  .view-all.text-center {
    @media (max-width: 767px) {      
      width: 100%;
      a {
        border-bottom: 1px solid $main-color;
        font-size: 16px;
        color: $main-color;
      }
    }
  }
  .home-banner-items {
    @media (max-width: 575px) {
      .row {
        @include margin-start(-7.5px);
        @include margin-end(-7.5px);
        >div {
          @include padding-start(7.5px);
          @include padding-end(7.5px);
        }
      }
    }
  }
  .home-banner-carousel {
    > .banner-item {
      display: none;
    }
    .banner-item {
      margin-bottom: 0;
    }
  }    
  &.effect-2 {    
    .banner-item {
      .image {
        a {           
          &:before {
            @extend %before;
          }
        }
        &:hover {
          @media (min-width: 1025px) {
            a {
              &:before {
                @include before-hover;
              }
            }
          }          
        }
      }
    }    
  }
  /*Effect Zoom Fade*/
  &.effect-3 {
    .banner-item {
      .image {
        img {
          @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
          opacity: 1;
        }
      }
      .text-box-default {
        @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
        margin-bottom: 3px;
      }
      &:hover {
        .image {
          img {
            opacity: 0.7;
            @include scale(1.1, 1.1);
          }
        }
        .text-box-default {
          @include box-shadow(1px 0px 4px rgba($overlay-color, 0.1));
        }
      }
    }
  }
  /*End Zoom Fade*/

  /*Effect Blur*/
  &.effect-4 {
    .banner-item {
      &:hover {            
        .image {
          img {
            @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
            -webkit-filter: blur(5px);
            filter: blur(5px);
          }
        }
      }
    }
  }
  /*End Blur*/

  /*Effect Glow*/
  &.effect-5 {
    .banner-item {
      &:hover {            
        .image {
          img {
            @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
            -webkit-filter: brightness(1.1);
            filter: brightness(1.1);
          }
        }
      }
    }
  }
  /*End Effect Glow*/

  /*Effect Color*/
  &.effect-6 {
    .banner-item { 
    @media(min-width: 1025px) {      
      .image {
        img {
          @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
          -webkit-filter: grayscale(100%);
          filter: grayscale(100%);
        }
      }
      &:hover {
        .image {
          img {
            -webkit-filter: grayscale(0);
            filter: grayscale(0);              }
          }
        }
    }          
      }
    }
    /*End Effect Color*/

    /*Effect Grayscale*/
    &.effect-7 {
      .banner-item {
        &:hover {
          .image {
            img {
              @include transition(filter .6s, opacity .6s, transform .6s, box-shadow .3s);
              -webkit-filter: grayscale(100%);
              filter: grayscale(100%);
            }
          }
        }
      }
    }
    /*End Grayscale*/
  &.layout-full {
    padding: 0;
  }         
}
