	.related-article-wrapper {
		.related-heading {
			padding-bottom: 30px;
			text-align: center;
		}
		.relatedthumb {
			position: relative;
			background: $light-color;
			margin-bottom: $padding-inset-sm;
			@include box-shadow(0 0 16px 5px rgba($dark-color, 0.05));
			.related-meta {
				padding: $padding-inset-md;
			}
			.infor-related {
				.post-title {
					text-align: start;
					font-weight: 400;
					display: block;
					margin: 0 0 11px;
					font-size: $size-h4;
					color: $body-title-color;
					&:hover {
						color: var(--main-color);
					}
				}
				.date {
					padding: 0 0 $padding-xxs;
					font-size: $body-font-size - 1px;
					color: $body-light-color;
					display: block;	
				}
				.read_more {
					a {
						text-transform: uppercase;
						color: var(--main-color);
					}
				}
			}
			.blog-excerpt {
				span {
					@extend %text-line-clamp;
					line-height: 22px;
					max-height: 66px;
					-webkit-line-clamp: 3;
					margin: 0 0 15px;
					color: $body-light-color;
					font-size: $body-font-size - 1px;
				}
