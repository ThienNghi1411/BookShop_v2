	.post-author-info {
		&.style-sidebar {
			border: 1px solid $body-border-color;
			.author-heading {
				padding: $padding-inset-sm;
				margin-bottom: 0;
				text-align: center;
				background: $body-title-color;
				color: color-contrast($body-title-color); 
			}
			.media {
				padding: $padding-inset-md;
				display: block;
				.image {
					width: 50%;
					margin: 0 auto;
					@extend %border-radius-circle;
				}
				.text {
					text-align: center;
					.author {
						padding: $padding-inset-md 0;
						font-weight: $heading-4-weight + 100;
					}
					.author-description {
						padding-bottom: $padding-inset-sm;
					}
					.member-social {
						a {
							font-size: $font-icon;
						}
					}
				}
