      .table-items {
        padding-bottom: 30px;
      }
      tr {
        @include flexbox; 
        align-items: center;
        border-bottom: 1px solid $body-border-color;
        position: relative;
        .item {
          @include flex-box(50%);
        }
        .price {
          @include flex-box(15%);
        }
        .qty {
          @include flex-box(20%);
        }
        .total-price {
          @include flex-box(15%);
        }
        td,th {
          border: 0;
          padding: $padding-inset-md 0 40px;
        }
      }
      .item-name {
        @include flex-box(50%);
        .item-image {
          width: 90px;
        }
        .item-title {
          @include padding-start($padding-inset-sm);
        }
      }
      .item-price {
        @include flex-box(15%);
        text-align: center;
      }
      .item-qty {
        @include flex-box(20%);
        display: flex;
        justify-content: center;
        align-items: center;
        .qty-wrapper {
          @include flexbox;
          align-items: center;
          width: 115px;
          border: 1px solid $body-border-color;
          @include border-radius(5px, 5px, 5px, 5px);
          >span {
            flex: 1;
            >span {
              height: 45px;
              width: 100%;
              @include flexbox;
              justify-content: center;
              align-items: center;
            }
          }
        }
        input {
          border-radius: 0;
          width: 40px;
          padding: 5px;
          text-align: center;
          height: 45px;
        }
      }
      .item-total {
        @include flex-box(15%);
        .action {
          @include ab(n,n,15px,0);
          .cart-remove-icon {
            color: $body-light-color;
            font-size: 12px;
            font-weight: $body-weight - 100;
            align-items: center;
            display: flex;
            i {
              background: $body-border-color;
              @include border-radius(50%, 50%, 50%, 50%);
              color: color-contrast($body-border-color);
              @include margin-end(6px);
              font-size: 10px;
              padding: 1px 0;
              width: 18px;
              height: 18px;
              line-height: 17px;           
            }
          }
          &:hover {
            .cart-remove-icon {
              color: var(--main-color);
              i {
                background: var(--main-color);
                color: color-contrast(var(--main-color));
              }
            }
          }
        }
      }
    }
    .sidebar-cart {
      border: 0;
      padding: 0;
      .sidebar-order-summary {
        border: 1px solid $body-border-color;
        @include border-radius(10px, 10px, 10px, 10px);
        >div {
          padding: $padding-inset-md;
        }
        .title-summary {
          background: var(--main-color);
          color: color-contrast(var(--main-color));
          @include border-radius(10px, 10px, 0, 0);
        }
        .cart-total {
          padding-bottom: 0;
        }
        .checkout-wrapper {
          text-align: center;
          .terms-conditions {
            text-align: start;
          }
          >a {
            width: auto;
            text-transform: capitalize;
          }
        }
      }
      .payment-image {
        border: 0;
        margin: 0;
      }
      .control-wrapper {
        .get-rates {
          width: auto;
          margin: 0 auto;
        }
      }
    }
    .cart-clear  {
      padding-bottom: 40px;
      @media (max-width: 767px) {
        padding-bottom: 20px;
      }
    }
  }
  .hide-mobile {
    @media (max-width: 767px) {
      display: none;
    }
  }
}
.page-cart {
  .cart-heading {
    display: none;
  }
}
@media (max-width: 767px) {
  .page-cart {
    .cart-heading {
      display: block;
    }
    .mobile-fixed {
      display: none;
      &.sticky {
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        width: 100%;
        padding: 15px;
        display: block;
        z-index: 999;
        -webkit-animation-name: fadeInDown;
        animation-name: fadeInDown;
        -webkit-animation-duration: .5s;
        animation-duration: .5s;
        -webkit-animation-fill-mode: both;
        animation-fill-mode: both;
        background: $body-second-color;
        @include box-shadow(0 0 15px $body-border-color);
      }
      .btn {
        width: 100%;
      }
      .terms-conditions {
        padding-bottom: 15px;
        text-align: start;
      }
    }
  }
}
.border-radius {
  @media (max-width: 767px) {    
    .page-cart {
      .cart-item {
        @include border-radius(5px, 5px, 5px, 5px);
      }
    }
  }
}
.templateCollection {
  .sb-banner {    
    > a {
      display: block;
      position: relative;
      &:before {
        @extend %before;
      } 
      img {
        width: 100%;
      }
      &:hover {
        @media (min-width: 1025px) {
          &:before {
            @include before-hover;
          }
        }
      }
    }    
  } 
}

.password-page__header__inner {
  padding: 15px 30px;
}
.password-login {
  text-align: end;
}
.password-login,
.admin-login {
  margin-top: 15px;
}
.template-password {
  height: 100vh;  
  width: 100%; 
  #shopify-section-password-content {
    text-align: center;
    padding: 0;
    margin: 0 auto;  
    background-image: url({{'password-page-background.jpg' | asset_url }});
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
    width: 100%;
    height: 100%;
    min-height: 100vh;  
  }
  .hidden-label {
    display: none;
  }
  *, input, *::before, *::after {
    box-sizing: border-box;
  }
  .password-page__main {
    height: 100%;
    margin: 0 auto;
    width: 100%; 
    margin-top: 8%;
    .password-content {
      margin: 0 auto;
      max-width: 530px;
      padding: 0 15px;
      text-align: center;
      .title-comming-soon {
        p {
          margin: 0;
          padding-bottom: 30px;
        }
        &:hover {
          @include text-shadow-ps;
        }
      }
      .offer-comming-soon {
        span:hover {
          @include text-shadow-ps;
        }
      }
    }
  }
  .mfp-preloader {
    display: none;
  }
  .countdown_password-content {
    padding-top: 40px;
    background: none;
    border: none;
    .countdown-row {
      @extend %flexbox;
      @extend %align-items-center;
      @extend %justify-content-center;
    }
    .countdown-section {      
      background-color: rgba(var(--main-color), 0.8);
      margin: 0 10px;
      width: calc(25% - 20px);
      @include flex(0 0 calc(25% - 20px));
      @include border-radius(4px, 4px, 4px, 4px);      
      @media (max-width: 479px) {        
        margin: 0 7px;
        width: calc(25% - 14px);
        @include flex(0 0 calc(25% - 14px));
      }
      &:hover {
        @include text-shadow-ps;
      }
      span {
        display: block;
      }
      .countdown-amount {
        font-size: 44px;
        font-weight: 500;
        padding: 10px;
        line-height: 100%;
        @media (max-width: 479px) {
          font-size: 28px;
        }
      }
      .countdown-period {
        font-size: 12px;
        padding: 10px 0;
        background: rgba(var(--main-color), 0.6);
        @include border-radius(0, 0, 4px, 4px);
      }
    }
  }
  .follow-us {
    padding-top: 40px;    
  }
}
.mfp-hide {
  display: none !important;
}
.mfp-bg {
  background: #ffffff none repeat scroll 0 0;
  height: 100%;
  left: 0;
  opacity: 1;
  overflow: hidden;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1042;
}
.mfp-wrap {
  height: 100%;
  left: 0;
  outline: medium none !important;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1043;
}
.mfp-container {
  background: $body-second-color;
  box-sizing: border-box;
  height: 100%;
  left: 0;
  padding: 0 8px;
  position: absolute;
  text-align: center;
  top: 0;
  width: 100%;
  &:before {
    content: "";
    display: inline-block;
    height: 100%;
    vertical-align: middle;
  }
}
button.mfp-close,
button.mfp-arrow {
  background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
  border: 0 none;
  box-shadow: none;
  cursor: pointer;
  display: block;
  outline: medium none;
  overflow: visible;
  padding: 0;
  z-index: 1046;
}
button::-moz-focus-inner {
  border: 0 none;
  padding: 0;
}
.mfp-close {
  color: #333333;
  font-family: Arial,Baskerville,monospace;
  font-size: 28px;
  font-style: normal;
  height: 44px;
  line-height: 44px;
  opacity: 0.65;
  padding: 0 0 18px 10px;
  position: absolute;
  right: 0;
  text-align: center;
  text-decoration: none;
  top: 0;
  width: 44px;
  &:hover,
  &:focus {
    opacity: 1;
  }
  &:active {
    top: 1px;
  }
}
.mfp-close-btn-in .mfp-close {
  color: #333333;
}
.mfp-align-top .mfp-container:before {
  display: none;
}
.mfp-content {
  display: inline-block;
  margin: 0 auto;
  position: relative;
  text-align: start;
  vertical-align: middle;
  z-index: 1045;
  color: #333;
}
.mfp-inline-holder,
.mfp-ajax-holder {
  .mfp-content {
    cursor: auto;
    width: 100%;
  }
}
.mfp-ajax-cur {
  cursor: progress;
}
.mfp-image-holder,
.mfp-iframe-holder {
  .mfp-close {
    color: #ffffff;
    @include padding-end(6px);
    right: -6px;
    text-align: end;
    width: 100%;
  }
}
button.mfp-close {
  color: #333333;
  font-size: 2.5em;
  margin: 0;
  opacity: 1;
}
.password-page__login-form {
