            @include padding-start($padding-inset-sm);
          }
        }
        .item-qty {
          @include flex-box(20%);
          display: flex;
          align-items: center;
          .qty-wrapper {
            @include flexbox;
            align-items: center;
            width: 115px;
            border: 1px solid $body-border-color;
            >span {
              flex: 1;
              cursor: pointer;
              >span {
                height: 45px;
                width: 100%;
                @include flexbox;
                justify-content: center;
                align-items: center;
              }
            }
          }
          input {
            border-radius: 0;
            width: 40px;
            padding: 5px;
            text-align: center;
          }
        }
        .item-total {
          @include flex-box(20%);
          .action {
            @include ab(0,n,n,0);
            background: var(--main-color)-text;
            border-radius: 50%;
