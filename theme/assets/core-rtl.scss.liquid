            height: 25px;
            line-height: 25px;
            @include padding-start(2px);
            a {
              color: color-contrast(var(--main-color)-text);
              .cart-remove-icon {
                font-size: 14px;
              }
            }
            &:hover {
              background: var(--main-color);
              a {
                color: color-contrast(var(--main-color));
              }
            }
          }
          .label-price {
            display: inline-block;
            float: left;
            text-align: end;
            .label {
              font-weight: $body-weight;
            }
            >span {
              display: block;
            }
          }
        }
      }
      .cart-clear {
        a {
          background: none;
          border: 0;
          padding: 10px 0;
          color: $body-title-color;
          text-transform: capitalize;
          box-shadow: unset;
          &:hover {
            background: none;
            box-shadow: unset;
            border: 0;
            color: inherit;
          }
        }
        >a {
          &:before {
            content: '\e82f';
            @include font-family;
            @include padding-start($padding-inset-xs);
            font-weight: 900;
          }
        }
      }
    }
    .cart-order-note {
      padding-top: 30px;
    }
    .sidebar-cart {
      .checkout-wrapper {
        .btn {
          @extend %border-radius-reset;
        }
      }
      #shipping-calculator {
        .control-wrapper {
          .get-rates {
            @extend %border-radius-reset;
          }
        }
      }
    }
    .cart-clear {
      a {
        @extend %border-radius-reset;
      }
    }
