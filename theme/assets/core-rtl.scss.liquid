}
.page-cart {
  padding-bottom: 60px;
  .cart-empty-wrapper {
    padding-top: 50px;
    @media (max-width: 767px) {
      padding-top: 30px;
    }
    >a {
      color: var(--main-color);      
      border-bottom: 1px solid currentColor;
      font-size: $body-font-size + 2px;
    }
  }
  #shipping-calculator {
    @media (max-width: 991px) {
      padding-bottom: $padding-inset-xs;
    }
    .shipping-calculator-form {
      padding-top: 15px;        
      .control-wrapper {
        select, input[type="text"] {
          @extend %border-radius-5;
        }
        &:last-child {
          padding-top: 0;
        }
      }
      &:not(.show) {
        @media (min-width: 768px) {
          display: block;
        }
      }       
    }
  }
  .cart-clear {
    @include flexbox;
    justify-content: space-between;
  }
  .cart-order-note {
    padding-bottom: $padding-lg;
    .cart-note {
      padding-top: 15px;
    }
    textarea {
      width: 100%;
      min-height: 120px;
    }
  }
  .payment-image {
    padding-bottom: $padding-inset-xs;
  }
  .free-shipping {
    @media (max-width: 767px) {
      padding-top: 25px;
    }
    .text {
      font-size: $body-font-size;
      padding-bottom: 15px;
    }
    .progress {
      @extend %load_progressbar;
      margin: 0 0 20px;
    }
    .progress-bar  {
      @extend %per_progressbar;
      color: color-contrast(var(--main-color));
    }
    .text span {
      font-weight: 700;
    }
  }
  .cart-total {
    @extend %flexbox;
    justify-content: space-between;
    padding-bottom: $padding-sm;
    font-size: $size-h4;
    >div {
      .money {
        font-weight: 600;
      }
    }
  }
  .checkout-wrapper {
    input[type="checkbox"] {
      margin-bottom: 1px;
    }
    .checkout-button {
      @media (min-width: 1440px) {
        @include flexbox;
        padding-top: 40px;
        .cart-clear {
          flex: 1;
          @include margin-start($padding-inset-xxs);
          a {
            padding: 9px;
            width: 100%;
          }
        }
        >a {
          flex: 1;
          padding: 9px;
          @include margin-end($padding-inset-xxs);
        }
      }
      @media (max-width: 1439px) {
        padding-top: 20px;
        .cart-clear {
          margin-top: 20px;
          a {
            width: 100%;
          }
        }
        >a {
          width: 100%;
        }
      }
    }
  }
  .action {
    [type=button], [type=reset], [type=submit], button {
      -webkit-appearance: none;
    }
    .cart-remove-icon {
      font-size: $size-h4;
    }
  }
  tr {
    .item-name-inner {
      @include flexbox;
      @include align-items(center);
    }
    .item-name {        
      .item-title {
        font-size: $product-font-size;
        font-weight: $product-title-weight;
        @media (max-width: 991px) {
          @include padding-start($padding-inset-xs);
          font-size: 14px;
        }
        @media (max-width: 767px) {
          display: none;
        }
      }
      .item-image {
        position: relative;
        &:before {
          content: '';
          display: block;
          @if $productImageRatioPercentage == 'auto' {
            padding-bottom: 100%;
            } @else if $productImageRatioPercentage == false {
              padding-bottom: 0%;
              } @else {
                padding-bottom: $productImageRatioPercentage;
              }
            }
            img {
              height: 100%;
              @if $productImageAutoCrop == false {
                object-fit: contain;
                font-family: "object-fit: contain";

              } @else {
                object-fit: cover;
                font-family: "object-fit: cover";
          }
        }
      }
    }
    .author-item {
      font-size: $body-font-size;
      @media (max-width: 991px) {
        font-size: 12px;
      }
    }
    .item-price, .item-total {
      font-size: $size-h4;
      color: $body-title-color;
      font-weight: $product-price-weight;
    }
    .item-qty {
      input {
        border: 0;
        @extend %border-radius-reset;
      }
    }
    @media (max-width: 767px) {
      &.cart-item {
        @include flexbox;
        align-items: center;
        @include box-shadow(0 0 2px $body-border-color);
        padding: 5px;
        margin-bottom: 15px;
      }
      td {
        padding: 0;
        border: 0;
      }
      .item-name {
        width: 90px;
        .item-image {
          width: 90px!important;
        }
      }
      .item-qty {
        text-align: start !important;
        @include padding-start($padding-inset-sm);
        flex: 1;
        .qty-wrapper {
          @include flexbox;
          border: 1px solid $body-border-color;
          width: 100px;
          margin: 5px 0;
          @include border-radius(5px, 5px, 5px, 5px);
          input {
            width: 34px!important;
            padding: 5px!important;
            height: 30px;
            text-align: center;
          }
          .qty-inner {
            @include flex-box(32px);
            @include flexbox;
            justify-content: center;
            align-items: center;
            height: 30px;
            background: rgba($body-border-color, 0.3);
          }
        }
      }
      .action {
        text-align: end!important;
        border: 0;
        @include flex-box(35px);
        .cart-remove-icon {
          background: var(--main-color);
          color: color-contrast(var(--main-color));
          font-size: 14px;
          padding: 3px 2px;
          @include border-radius(5px, 5px, 5px, 5px);
        }
      }
    }
  }
  @media (max-width: 575px) {
    .cart-clear {
      a {
        padding: 9px 15px;
      }
    }
  }
  @media (max-width: 369px) {
    .cart-clear {
      a {
        padding: 9px;
        font-size: 13px;
      }
    }
  }
  .sidebar-cart {
    border: 1px solid $body-border-color;
    padding: $padding-inset-md;
    .title-summary {
      font-size: $size-h4 + 2px;
      font-weight: $body-weight + 100;
      padding-bottom: $padding-xs;
    }
    .checkout-wrapper {
      .btn {
        margin-top: $padding-md;
        width: 100%;
      }
    }
    .payment-image {
      padding-bottom: $padding-xs;
      border-top: 1px solid $body-border-color;
      margin-top: 20px;
      >span {
        padding: $padding-md 0;
        display: block;
        font-size: $size-h4;
        text-align: center;
        font-weight: $body-weight + 100;
      }
    }
  }
  &.cart-style-book {
    table tr th, table tr td {
      text-align: center;
    }
    .table-items {
      padding-bottom: 30px;
      @media (max-width: 767px) {
        padding-bottom: 10px;
      }
    }
    .cart-table .table-items~.cart-clear {
      padding-bottom: 50px;
      @media (max-width: 767px) {
        padding-bottom: 30px;
      }
      >a {
        display: none;
      }
    }
    tr {
      .item-name {        
        .item-image {
          width: 70px;
          min-width: 70px;
        }
        .item-title {
          @include padding-start($padding-inset-lg);
          text-align: start;
        }
      }
      .item-qty {
        input {
          width: 55px;
          text-align: center;
          font-size: $size-h4;
          @media (max-width: 991px) {
            width: 50px;
            padding: 10px;
          }
        }
        .qty-inner {
          font-size: $size-h4;
          cursor: pointer;
          .qty-down {            
            i {
              &:before {
                content: "\e80a";
                @extend %font-family;
              }
            }
          }
          .qty-up {
            i {
              &:before {
                content: "\e83c";
                @extend %font-family;
              }
            }
          }
        }
      }
    }
  }
  &.cart-style-art {
    @media (min-width: 768px) {
    .table-items {
      border: 1px solid $body-border-color;
      padding: $padding-inset-md;
    }      
      tr {
        @include flexbox;
        border-bottom: 1px solid $body-border-color;
        margin-bottom: $padding-inset-md;
