/*calculate YIQ and return text color follow Background*/
@function color-contrast($color, $dark: $dark-color, $light: $light-color) {
  @if $color == null {
    @return null;
  }
  @else {
    $yiq: (red($color) * 299 + green($color) * 587 + blue($color) * 114) / 1000;
    @return if($yiq >= 128, $dark, $light);
  }
}
@function px-to-rem($target, $context: $body-font-size) {
  @return ($target / $context) * 1rem;
}
