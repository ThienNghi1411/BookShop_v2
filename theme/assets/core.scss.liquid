#sidebar-blog {
	.sb-blog-socials {
		ul {
			li {
				border: none;
				padding: 0;
				@include margin-end($padding-inset-xxs);
				margin-bottom: 0;				
			}
		}
	}
	.sb-blog-tag {
		ul {
			li {
				padding: 7px 20px;
				@include margin-end($padding-inset-xs);
				margin-bottom: 10px;
				border: 1px solid $body-general-color;
				text-align: center;
				@include transition(0.35s all ease-in-out);
				@extend %border-radius-5;
				&:hover {
					background: $main-color;
					border: 1px solid $main-color;
					a {
						color: color-contrast($main-color);
					}
				}
			}
		}
	}
	&.title-sidebar-center {
		.sb-title {
			text-align: center;
		}
	}
	&.title-sidebar-left {
		.sb-title {
			text-align: start;
		}
	}
	&.title-sidebar-right {
		.sb-title {
			text-align: end;
		}
	}
	.categories-menu {
		text-align: start;
	}
	.sidebar-style-1 {
		.sb-blog-posts {
			.post-item-inner {
				.post-image {
					max-width: 100%;
					height: 80px;
					span {
						height: 100%;
						img {
							height: 100%;
						}
					}
				}
				.post-content {
					@include padding-start($padding-inset-md);
					padding-top: 0;
				}
			}
		}
		&:not(.has-border) {
			.sb-widget {
				.sb-blog-posts {
					.post-item:not(:last-child) {
						margin-bottom: 40px;
					}
				}
			}
		}
	}
	.sidebar-style-2 {
		.sb-blog-posts {
			.post-item-inner {
				display: block;
				.post-image {
					max-width: 100%;
				}
				.post-content {
					padding-top: $padding-inset-sm;
					padding-bottom: $padding-inset-sm;
					.info {
						h6 {
							padding-bottom: $padding-inset-xs;
							font-weight: $heading-6-weight + 200;
						}
					}
				}
			}
		}
		&.has-border {
