      @include flex(0 0 calc(25% - 20px));
      @include border-radius(4px, 4px, 4px, 4px);      
      @media (max-width: 479px) {        
        margin: 0 5px;
        width: calc(25% - 14px);
        @include flex(0 0 calc(25% - 14px));
      }
      &:hover {
        @include text-shadow-ps;
      }
      span {
        display: block;
      }
      .countdown-amount {
        font-size: 44px;
        font-weight: 500;
        padding: 10px;
        line-height: 100%;
        @media (max-width: 479px) {
          padding: 12px 4px;
          font-size: 24px;
        }
      }
      .countdown-period {
        font-size: 12px;
        padding: 10px 0;
        background: rgba($main-color, 0.6);
        @include border-radius(0, 0, 4px, 4px);
        @media (max-width: 479px) {
          padding: 5px 0;
        }
      }
    }
  }
  .follow-us {
    padding-top: 40px;  
    .share-links {
      li {
        padding: 0 15px;
        @media (max-width: 575px) {
          padding: 0 5px;
        }
        a {
          min-width: 120px;
          @media (max-width: 575px) {
            min-width: 50px;
            i {
              display: block;
            }
            span {
              display: none;
            }
          }
        }
      }
    }  
  }
}
.mfp-hide {
  display: none !important;
}
.mfp-bg {
  background: #ffffff none repeat scroll 0 0;
  height: 100%;
  left: 0;
  opacity: 1;
  overflow: hidden;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1042;
}
.mfp-wrap {
  height: 100%;
  left: 0;
  outline: medium none !important;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1043;
}
.mfp-container {
  background: $body-second-color;
  box-sizing: border-box;
  height: 100%;
  left: 0;
  padding: 0 15px;
  position: absolute;
  text-align: center;
  top: 0;
  width: 100%;
  &:before {
    content: "";
    display: inline-block;
    height: 100%;
    vertical-align: middle;
  }
}
button.mfp-close,
button.mfp-arrow {
  background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
  border: 0 none;
  box-shadow: none;
  cursor: pointer;
  display: block;
  outline: medium none;
  overflow: visible;
  padding: 0;
  z-index: 1046;
}
button::-moz-focus-inner {
  border: 0 none;
  padding: 0;
}
.mfp-close {
  color: #333333;
  font-family: Arial,Baskerville,monospace;
  font-size: 28px;
  font-style: normal;
  height: 44px;
  line-height: 44px;
  opacity: 0.65;
  padding: 0 0 18px 10px;
  position: absolute;
  right: 0;
  text-align: center;
  text-decoration: none;
  top: 0;
  width: 44px;
  &:hover,
  &:focus {
    opacity: 1;
  }
  &:active {
    top: 1px;
  }
}
.mfp-close-btn-in .mfp-close {
  color: #333333;
}
.mfp-align-top .mfp-container:before {
  display: none;
}
.mfp-content {
  display: inline-block;
  margin: 0 auto;
  position: relative;
  text-align: start;
  vertical-align: middle;
  z-index: 1045;
  color: #333;
}
.mfp-inline-holder,
.mfp-ajax-holder {
  .mfp-content {
    cursor: auto;
    width: 100%;
  }
}
.mfp-ajax-cur {
  cursor: progress;
}
.mfp-image-holder,
.mfp-iframe-holder {
  .mfp-close {
    color: #ffffff;
    @include padding-end(6px);
    right: -6px;
    text-align: end;
    width: 100%;
  }
}
button.mfp-close {
  color: #333333;
  font-size: 2.5em;
  margin: 0;
  opacity: 1;
}
.password-page__login-form {
  max-width: 500px;
  margin: 0 auto;
  >p {
    text-align: center;
    padding-bottom: 30px;
  }
  .errors {
    text-align: center;
    ul {
      list-style-type: none;
      @include margin-start(0);
    }
  }
  .admin-login {
    text-align: center;
  }
  .input-group {
    position: relative;    
  }
  .input-group-field {
    border: 1px solid $body-border-color;
    height: 44px;
    line-height: 20px;
    padding: 10px 15px;
    @include flex(1);
    @media (max-width: 479px) {
      @include flex(0 0 100%);
    }
  }
  .input-group-btn {
    @media (max-width: 479px) {
      width: 100%;
      text-align: center;
    }
    input {      
      font-size: $body-font-size;
      padding: 7px 25px;
      height: 42px;      
      cursor: pointer;   
      @include margin-start($padding-inset-md);   
      @extend %btn-1; 
      @extend %border-radius-30; 
      @media (max-width: 479px) {
        margin: 10px auto 0;
      }
    }
  }
}

